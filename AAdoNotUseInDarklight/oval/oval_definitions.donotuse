    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#anySimpleType -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#anySimpleType"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://oval.mitre.org/definitions#affected -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#affected">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#AffectedSystem"/>
        <rdfs:comment>Specifes information about the system(s) for which the definition has been written. Remember that this is just metadata and not part of the criteria. 

The whose main purpose is to provide hints for tools using OVAL Definitions. For instance, to help a reporting tool only use Windows definitions, or to preselect only Red Hat definitions to be evaluated. Note, the inclusion of a particular platform or product does not mean the definition is physically checking for the existence of the platform or product. For the actual test to be performed, the correct test must still be included in the definition&apos;s criteria section</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Affected</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#criteria -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#criteria">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#Criteria"/>
        <rdfs:comment>Specifies the set of criteria associated</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>criteria</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#criterion -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#criterion">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#Criterion"/>
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>criterion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#definitionReference -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#definitionReference">
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>definition reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#extendedDefinition -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#extendedDefinition">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#ExtendedDefinition"/>
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Extended Definition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#lliteralComponent -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#lliteralComponent">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#LiteralComponent"/>
        <rdfs:comment>Specifies a literal component</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>lliteral component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#localVariableValue -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#localVariableValue">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://oval.mitre.org/definitions#ArithmeticFunction"/>
                    <rdf:Description rdf:about="http://oval.mitre.org/definitions#LiteralComponent"/>
                    <rdf:Description rdf:about="http://oval.mitre.org/definitions#ObjectComponent"/>
                    <rdf:Description rdf:about="http://oval.mitre.org/definitions#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Specifies where to obtain the value of the local variable</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>local variable value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#notes -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#notes">
        <rdfs:comment>Specifies associated notes</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#objectComponent -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#objectComponent">
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>object component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#objectReference -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#objectReference">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#Object"/>
        <rdfs:comment>Specifies a reference to an existing OVAL Object declaration. The referenced OVAL Object specifies a set of OVAL Items to collect. Note that an OVAL Object might identify 0, 1, or many OVAL Items on a system. If no items are found on the system then an error should be reported when determining the value. If 1 or more OVAL Items are found then each OVAL Item will be considered and the ObjectComponent may have one or more values.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Object Reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#possibleRestriction -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#possibleRestriction">
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>possible restriction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#possibleValue -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#possibleValue">
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>possible value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#reference -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#reference">
        <rdfs:range rdf:resource="http://oval.mitre.org/definitions#Reference"/>
        <rdfs:comment>Specifies one more external references.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#testReference -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#testReference">
        <rdfs:comment>Specifies a reference to the actual test beign referenced.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Test Reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://oval.mitre.org/definitions#variableReference -->

    <owl:ObjectProperty rdf:about="http://oval.mitre.org/definitions#variableReference">
        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&lt;http://oval.mitre.org/definition&gt;</rdfs:isDefinedBy>
        <rdfs:label>variable reference</rdfs:label>
    </owl:ObjectProperty>
    



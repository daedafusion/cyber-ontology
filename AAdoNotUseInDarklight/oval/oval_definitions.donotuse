                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Characterizes a container for a set of sub criteria, criteria, criterion, or extend_definition elements allowing complex logical trees to be constructed. Each referenced test is represented by a criterion element.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://oval.mitre.org/definitions"/>
        <rdfs:label>Criteria</rdfs:label>
    </owl:Class>
    


    <!-- http://oval.mitre.org/definitions#Criterion -->

    <owl:Class rdf:about="http://oval.mitre.org/definitions#Criterion">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://oval.mitre.org/definitions#testReference"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://oval.mitre.org/definitions#applicabilityCheck"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://oval.mitre.org/definitions#comment"/>

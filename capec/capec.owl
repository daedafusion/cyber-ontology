        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the name associated with the Functional Service</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name</rdfs:label>
    </owl:DatatypeProperty>
    



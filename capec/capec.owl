                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Internal_CAPEC_Team</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >External_Submission</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"

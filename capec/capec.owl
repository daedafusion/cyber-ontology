    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#imageTitle">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a brief title associated with the image</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Image Title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#impact.availability -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#impact.availability">

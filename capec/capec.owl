  <owl:DatatypeProperty rdf:ID="relationshipType">
    <rdfs:label >Relationship Type</rdfs:label>
    <rdfs:comment >Specifies the type of relationship</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Is a command of</rdf:first>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Is a service of</rdf:first>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Uses Protocol</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Is an alternative to</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Is a refinement of</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Is an abstraction of</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:domain rdf:resource="#RelatedProtocolStatement"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#weaknessRelationshipType">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Secondary</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Targeted</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label >Relationship Type</rdfs:label>
    <rdfs:comment >Specifies the nature of the relationship between this weakness and the 	attack pattern. Weaknesses that are specifically targeted by the attack are of type â€œTargetedâ€.  Weaknesses which are not specifically targeted but whose presence may increase the likelihood of the attack succeeding or the impact of the attack if it does succeed are of type â€œSecondaryâ€.</rdfs:comment>
    <rdfs:domain rdf:resource="#RelatedWeaknessStatement"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="relevantDesignPatterns.nonRecommended">
    <rdfs:domain rdf:resource="#AttackPattern"/>

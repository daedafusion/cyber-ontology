     -->


    


    <!-- http://capec.mitre.org/capec#activationZone -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#activationZone">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#structuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the area within the target software that is capable of executing or otherwise activating the payload of an injection-based attack of this type. The activation zone is where the intent of the attacker is put into action. The activation zone may be a command interpreter, some active machine code in a buffer, a client browser, a system API call, etc.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Activation Zone</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#attackPrerequisites -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#attackPrerequisites">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#structuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the conditions that must exist or the functionality and characteristics that the target software must have or behavior it must exhibit for an attack of this type to succeed.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Prerequisites</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#attackSurfaceId -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#attackSurfaceId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies an identifier for the attack surface</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Surface Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#authorName -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#authorName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the name of the author for this entry</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#authorOganizationName -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#authorOganizationName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the name of the author&apos;s organization</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author&apos;s Oganization Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#backgroundDetails -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#backgroundDetails">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#structuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Speicfies information regarding the entry or any technologies that are related to it, where the background information is 	not related to the nature of the entry itself. It should be filled out where appropriate.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Background Details</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#categoryName -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#categoryName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a descriptive name used to give the reader an idea of what the commonality is amongst the children of this category. 

All words in the name should be capitalized except for articles and prepositions unless they begin or end the name. Subsequent words in a hyphenated chain are also not capitalized.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#code -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#code">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a line of code.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#codeComment -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#codeComment">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a comment in code.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#codeExampleLanguage -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#codeExampleLanguage">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the programming language being used in the following block of Code</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Code Example Language</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#commandDescription -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#commandDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a brief description of the command structure</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#commandGroupLabel -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#commandGroupLabel">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the group label for the command structure</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Group Label</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#commandStructureId -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#commandStructureId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the identifier for a Command Structure</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Command Structure Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#commandStructureName -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#commandStructureName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a name associated with the Command Structure</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#commandType -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#commandType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the type of the command structure</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#compoundElementAbstraction -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#compoundElementAbstraction">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the abstraction level for this attack pattern. The abstraction levels for Compound Elements and Attack Patterns 	are the same. For example, if the Compound Element is a chain, and all elements of the chain are Meta level, then the Compound Element Abstraction attribute is Meta.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Element Abstraction</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#compoundElementCompleteness -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#compoundElementCompleteness">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the completeness of the Compound Element</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Element Completeness</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#compoundElementStructure -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#compoundElementStructure">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies  the structural nature of this Compound Element - that is, composed of other attack patterns concurrently,	as in a composite, or consecutively, as in a chain.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Element Structure</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#consequenceId -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#consequenceId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the value for the related Common_Consequence entry identifier as a string.  Only one Common Consequence Identifier can exist for each Consequence element (ex: CC-1).  However, Consequences across CAPEC with the same ID should only vary in small details.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequence Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#consequenceNote -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#consequenceNote">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#structuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies additional commentary about this consequence.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequence Note</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#consequenceScope -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#consequenceScope">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies an individual consequence that may result from this attack pattern.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Scope</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#contributionImportance -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#contributionImportance">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies how significant the modification is with regard to the meaning and interpretation of the associated entity.  If a modification has a value of Critical, then the meaning of the entry or how it might be interpreted has changed and requires attention from anyone previously dependent on the associated entity.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Contribution Importance</rdfs:label>
    </owl:DatatypeProperty>
    



        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the location of the image file</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Image Location</rdfs:label>
    </owl:DatatypeProperty>
    



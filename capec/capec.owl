        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the identifier for the Funcational Service</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Functional Service Identifier</rdfs:label>
    </owl:DatatypeProperty>
    



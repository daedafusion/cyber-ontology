    <rdfs:domain rdf:resource="#View"/>
    <rdfs:comment xml:lang="en-US">Specifies details regarding the relationships between CAPEC entries.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="attackPrerequisites">
    <rdfs:label xml:lang="en-US">Attack Prerequisites</rdfs:label>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies the conditions that must exist or the functionality and characteristics that the target software must have or behavior it must exhibit for an attack of this type to succeed.</rdfs:comment>

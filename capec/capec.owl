<?xml version="1.0"?>
<rdf:RDF xmlns="http://capec.mitre.org/capec#"
     xml:base="http://capec.mitre.org/capec"
     xmlns:core="tag:champtc:core#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://capec.mitre.org/capec">
        <owl:imports rdf:resource="http://cybox.mitre.org/objects/URI"/>
        <owl:imports rdf:resource="http://scap.nist.gov/scap_core"/>
        <owl:imports rdf:resource="http://cybox.mitre.org/cybox_core"/>
        <owl:imports rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol"/>
        <owl:imports rdf:resource="http://nvd.nist.gov/cve"/>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">v1.0.0-001</owl:versionInfo>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Attack Pattern Enumeration and Classification (CAPEC) Ontology</rdfs:label>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology provides a vocabulary and representation for the Common Attack Pattern Enumeration and Classification (CAPEC) which provides a publicly available, community-developed list of common attack patterns along with a comprehensive schema and classification taxonomy. Attack patterns are descriptions of common methods for exploiting software systems. They derive from the concept of design patterns applied in a destructive rather than constructive context and are generated from in-depth analysis of specific real-world exploit examples.

Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- tag:champtc:core#appIconId -->


    <owl:AnnotationProperty rdf:about="tag:champtc:core#appIconId"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://capec.mitre.org/capec#structuredText -->


    <rdfs:Datatype rdf:about="http://capec.mitre.org/capec#structuredText">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies that the text description is formatted utilizing some encoding scheme.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://capec.mitre.org/capec"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Structured Text</rdfs:label>
    </rdfs:Datatype>
    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://capec.mitre.org/capec#alternateTerms -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#alternateTerms">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Term"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies zero or more Alternate Terms, each ofwhich contains other names used to describe this attack pattern.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alternate Terms</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#attackExecutionFlow -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#attackExecutionFlow">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackPhase"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a detailed description of the attack including the chain of actions taken by the attacker. More comprehensive descriptions could include relevant attack trees and/or exploit graphs to more clearly elaborate this type of attack.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Execution Flow</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#attackMotivationConsequences -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#attackMotivationConsequences">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Consequence"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies what the attacker is trying to achieve by using this attack.  This is not the end business/mission goal of the attack within the target context but rather the specific technical result desired that 	could be leveraged to achieve the end business/mission objective. This information is useful for aligning attack patterns to threat models and for determining which attack patterns are relevant for a given context.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Motivation Consequences</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#attackPatterns -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#attackPatterns">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackPattern"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a collection of attack patterns</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Patterns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#attackSteps -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#attackSteps">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackStep"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the steps of the attack pattern</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Steps</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#attackTechniques -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#attackTechniques">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackStepTechnique"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies various techniques that the attacker can use to achieve the attack step’s goal. For example, an attacker may use tools such as WebScarab and Tamper Data in the experimentation phase of a SQL Injection attack pattern. The techniques include references to environments, because not all techniques work in all environments</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Techniques</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#attackerKnowledgeRequired -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#attackerKnowledgeRequired">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Knowlege"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies  the level of skills or specific knowledge required by an attacker to execute this type of attack.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attacker Skill or Knowledge Required</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#audience -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#audience">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Audience"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to the targeted audiences or groups for this view.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Audience</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#author -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#author">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Organization"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Party"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the author</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#authorOrganization -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#authorOrganization">
        <rdfs:range rdf:resource="http://www.oasis-open.org/ciq_xpil#Organization"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the author&apos;s organization</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author&apos;s Organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#categories -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#categories">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Category"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies  a collection of attack patterns sharing a common attribute. The shared attribute may any number of things.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Categories</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#commonAttackSteps -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#commonAttackSteps">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackStep"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a collection of attack steps</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Attack Steps</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#comonAttackSurfaces -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#comonAttackSurfaces">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#TargetAttackSurfaceDescription"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a collection of common attack surfaces</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comon Attack Surfaces</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#compoundElements -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#compoundElements">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#CompoundElement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a meaningful aggregation of several attack patterns.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Elements</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#consequenceTechnicalImpact -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#consequenceTechnicalImpact">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#TechnicalImpact"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the technical impacts that can result from successful execution of this attack pattern.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Technical Impact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#contentHistory -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#contentHistory">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#HistoryRecord"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more history records  used to keep track of the author of the attack pattern entry and anyone who has made modifications to the content. This provides a means of contacting the authors and  modifiers for clarifying ambiguities, merging overlapping contributions, etc. This should be filled out for all entries.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Content History</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#environments -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#environments">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Environment"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the environments where an attack step technique is applicable.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Environments</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#exampleInstances -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#exampleInstances">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#ExampleInstance"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies explanatory examples or demonstrative exploit instances of this attack, which are intended to help the reader understand the nature, context and variability of the attack in more practical and concrete terms.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Examples Instances</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#indicatorWarningOfAttack -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#indicatorWarningOfAttack">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#IndicatorWarning"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more indicator warning of attack. Indicator warning of attack describes activities, events,	conditions or behaviors that may indicate that an attack of this type is imminent, in progress or has occurred.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicator-Wwarning of Attack</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#indicators -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#indicators">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#SusceptabiltyIndicator"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies indicators that the application may or may not be susceptible to the given attack step (not necessarily the pattern as a whole).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicators</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#leveragedAttackPatterns -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#leveragedAttackPatterns">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackPattern"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies any attack patterns which are leveraged</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Leveraged Attack Patterns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#nonPrimaryViews -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#nonPrimaryViews">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#View"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a list of the individual Views to which this relationship pertains.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Non-Primary Related Views</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#obfuscationTechniques -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#obfuscationTechniques">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#ObfuscationTechnique"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more obfuscation techniques. An obfuscation technique can be used to disguise the fact that an attack of this type is imminent, in progress or has occurred.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Obfuscation Techniques</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#observableEvidenceIfNotPresent -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#observableEvidenceIfNotPresent">
        <rdfs:range rdf:resource="http://cybox.mitre.org/cybox_core#Observables"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies observable evidence, if not present</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observable Evidence If Not Present</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#observableEvidenceIfPresent -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#observableEvidenceIfPresent">
        <rdfs:range rdf:resource="http://cybox.mitre.org/cybox_core#Observables"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies observable evidence, if present</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observable Evidence If Present</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#observables -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#observables">
        <rdfs:range rdf:resource="http://cybox.mitre.org/cybox_core#Observables"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies observable behaviors</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observables</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#outcomes -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#outcomes">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Outcome"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies possible outcomes for this attack step.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Outcomes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#patternSpecificOverrides -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#patternSpecificOverrides">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#TargetAttackSurfaceDescription"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies pattern-specific overrides for a target attack surface</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pattern-Specific Overrides</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#payloadActivationImpact.observables -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#payloadActivationImpact.observables">
        <rdfs:range rdf:resource="http://cybox.mitre.org/cybox_core#Observables"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies detailed cyber observable patterns for potential detection of the payload activation impact.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://capec.mitre.org/capec"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Payload Activation Impact Observables</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#primaryViews -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#primaryViews">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#View"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a list of the individual Views to which this relationship pertains</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://capec.mitre.org/capec"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Primary Related Views</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#probingTechniques -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#probingTechniques">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#ProbingTechnique"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more probing techniques. A probing technique describes a method used to probe and reconnoiter a potential target to determine vulnerability and/or to prepare for this type of attack.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Probing Techniques</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#protocol -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#protocol">
        <rdfs:range rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol#NetworkProtocol"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a Protocol</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Protocol</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#referenceAuthor -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#referenceAuthor">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Organization"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Party"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to one or more authors of the material being referenced.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#referencePublisher -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#referencePublisher">
        <rdfs:range rdf:resource="http://www.oasis-open.org/ciq_xpil#Organization"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the publisher of the reference material, if one exists.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Publisher</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#references -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#references">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#Reference"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more references.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">References</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relatedAttackPatterns -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relatedAttackPatterns">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackPattern"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more related attack patterns. A related attack pattern refers to an attack pattern that is dependent on or applied in conjunction with this attack pattern.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Related Attack Patterns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relatedProtocols -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relatedProtocols">
        <rdfs:range rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol#NetworkProtocol"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies zero or more protocols related to the existing protocol</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Related Protocols</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relatedVulnerabilities -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relatedVulnerabilities">
        <rdfs:range rdf:resource="http://nvd.nist.gov/cve#Vulnerability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies  one or more related vulnerabilities. A related vulnerability refers to a specific instance vulnerability targeted for exploit by this attack pattern. 

USAGE: This element is used to identify specific vulnerabilities by their 	industry-standard Common Vulnerabilities and Exposures (CVE) numbers and/or US-CERT numbers. As vulnerabilities are much more specific and localized than weaknesses, it is uncommon that an attack pattern would target a specific vulnerability. This would most likely occur if the attack pattern were targeting vulnerabilities in the underlying platform, framework, or software library.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Related Vulnerability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relatedWeaknesses -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relatedWeaknesses">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://cwe.mitre.org/cwe#Category"/>
                    <rdf:Description rdf:about="http://cwe.mitre.org/cwe#CompoundElement"/>
                    <rdf:Description rdf:about="http://cwe.mitre.org/cwe#View"/>
                    <rdf:Description rdf:about="http://cwe.mitre.org/cwe#Weakness"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the specific weaknesses the attack targets and leverages.

Specific weaknesses (underlying issues that may cause vulnerabilities) reference the industry-standard Common Weakness 	Enumeration (CWE). This list should include not only those weaknesses that are directly targeted by the attack but also those whose presence can directly increase the likelihood of the attack succeeding or the impact if it does succeed.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Related Weaknesses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relationshipChains -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relationshipChains">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#CompoundElement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a list of the individual Chains this relationship pertains to.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship Chains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relationships -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relationships">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://capec.mitre.org/capec#AttackPattern"/>
                    <rdf:Description rdf:about="http://capec.mitre.org/capec#Category"/>
                    <rdf:Description rdf:about="http://capec.mitre.org/capec#CompoundElement"/>
                    <rdf:Description rdf:about="http://capec.mitre.org/capec#View"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specfies one or more Relationship elements, each of which identifies an association between this structure, whether it is a Attack Pattern, Category, or Compound_Element and another structure.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationships</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relevantCommandStructures -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relevantCommandStructures">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#CommandStructure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies command structures that are relevant to this attack surface</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relevant Command Structures</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relevantFunctionalServices -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relevantFunctionalServices">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#FunctionalService"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the functional services that are relevant to this attack surface</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relevant Functional Services</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relevantProtocolHeaders -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relevantProtocolHeaders">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies protocol headers that a relevant to this attack surface</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relevant Protocl Headers</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#relevantProtocols -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#relevantProtocols">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#TargetedProtocol"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies protocols that are relevant to this attack surface area.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relevant Protocols</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#securityControls -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#securityControls">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#SecurityControl"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies security controls for this attack step that describe ways in which the attack step can be detected, corrected, or prevented. These are presented from a defender’s point of view, where the defender may be a developer, tester, operations administrator, or other resource resisting the attacker.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Security Controls</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#targetAttackSurface -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#targetAttackSurface">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://capec.mitre.org/capec#CommonAttackSurfaceDescription"/>
                    <rdf:Description rdf:about="http://capec.mitre.org/capec#TargetAttackSurfaceDescription"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the locations where an attacker interacts with the target system</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Target Attack Surface</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#targetFunctionalServices -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#targetFunctionalServices">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#FunctionalService"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies Functional Services that are part of the target attack surface</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Surface Target Functional Services</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#targetedCommandStructures -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#targetedCommandStructures">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#CommandStructure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the protocol&apos;s command structures that are being targeted by the attack pattern</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TargetedCommand Structures</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#targetedProtocolFields -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#targetedProtocolFields">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies zero or more specific fields of the protocol being utilized</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Targeted Protocol Fields</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#targetedProtocols -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#targetedProtocols">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#TargetedProtocol"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more protocols associated with the Functional Service that an attack pattern specifically targets aspects of the protocol</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Targeted Protocols</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#usesProtocols -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#usesProtocols">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more protocols associated with the Functional Service that are generally targeted by an attack pattern</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Uses Protocols</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#views -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#views">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#View"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to one or more Attack Pattern Views</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Views</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://capec.mitre.org/capec#vulnerability -->


    <owl:ObjectProperty rdf:about="http://capec.mitre.org/capec#vulnerability">
        <rdfs:range rdf:resource="http://nvd.nist.gov/cve#Vulnerability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the vulnerability</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vulnerability</rdfs:label>
    </owl:ObjectProperty>
    



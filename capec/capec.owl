        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Speifies an explanation for the likelihood of Exploit</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Likelyhood of Exploit Explanation</rdfs:label>
    </owl:DatatypeProperty>
    



        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the identifer of the Attack Pattern, Category, View</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attack Pattern Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#imageLocation -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#imageLocation">

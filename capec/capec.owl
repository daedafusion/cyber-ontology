  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#outcomeDescription">
    <rdfs:label xml:lang="en-US">Description</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a brief description of the outcome</rdfs:comment>
    <rdfs:domain rdf:resource="#Outcome"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="codeComment">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Comment</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a comment in code.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="otherNotes">
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:comment xml:lang="en-US">Specifies one or more Note elements, each of which provide any additional notes or comments that cannot be captured using other elements. New elements might be defined in the future to contain this information. It should be filled out where needed.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Other Notes</rdfs:label>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="relatedGuidelines">
    <rdfs:label xml:lang="en-US">Related Guidelines</rdfs:label>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies one or more related guidelines. A related guideline represents a security guideline that is relevant to identifying or mitigating this type of attack. &#xD;
&#xD;
USAGE: It would be helpful to provide a usage reference.  However links to security principle and guideline documentation on the BSI site appear to be broken. NIST SP 800-27 uses the terms principle and guideline interchangeably.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#outcomeId">
    <rdfs:domain rdf:resource="#Outcome"/>
    <rdfs:comment xml:lang="en-US">Specifies a unique integer identifier for the outcome.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en-US">Outcome Identifier</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#referenceId">
    <rdfs:label xml:lang="en-US">Reference Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a mechanism forciting text in the entry. If an id is provided, it is placed between brackets and precedes this reference and the matching id should be used inside of the text for the attack pattern itself where this reference is applicable. All reference ids assigned within an entry must be unique.</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#entryType">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Modification</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Contributions</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Submission</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en-US">Entry Type</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the type of history records.</rdfs:comment>
    <rdfs:domain rdf:resource="#HistoryRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#skillKnowledgeDescription">
    <rdfs:domain rdf:resource="#Knowlege"/>
    <rdfs:comment xml:lang="en-US">Specifies contextual detail for the skill or knowledge level.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Skill or Knowledge Description</rdfs:label>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="indicatorDescription">
    <rdfs:label xml:lang="en-US">Description</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a brief description of the indicator.</rdfs:comment>
    <rdfs:domain rdf:resource="#SusceptabiltyIndicator"/>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandStructureId">
    <rdfs:label xml:lang="en-US">Command Structure Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the identifier for a Command Structure</rdfs:comment>
    <rdfs:domain rdf:resource="#CommandStructure"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#environmentTitle">
    <rdfs:label xml:lang="en-US">Title</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a short descriptive title for the environment</rdfs:comment>
    <rdfs:domain rdf:resource="#Environment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="maintenanceNotes">
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:label xml:lang="en-US">Maintenance Notes</rdfs:label>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:comment xml:lang="en-US">Specifies one or more maintenance notes which each contain significant maintenance tasks within this entry that still need to be addressed, such as clarifying the concepts involved or improving relationships. It should be filled out in any entry that is still undergoing significant review by the CAPEC team.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#referenceSection">
    <rdfs:label xml:lang="en-US">Section</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a means of identifying the exact location of the material inside of the publication source, such as the relevant pages of a research paper, the appropriate chapters from a book, etc. This is useful for both book references and internet references.</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#phaseName">
    <rdfs:domain rdf:resource="#AttackPhase"/>
    <rdfs:comment xml:lang="en-US">Specifies the name of the attack phase</rdfs:comment>
    <rdfs:label xml:lang="en-US">Attack Phase Name</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Explore</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Experiment</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Exploit</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#injectionVector">
    <rdfs:label xml:lang="en-US">Injection Vector</rdfs:label>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies the mechanism and format of an input-driven attack of this type. Injection vectors take into account the grammar of an attack, the syntax accepted by the system, the position of various fields, and the ranges of data that are acceptable.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="authorName">
    <rdfs:comment xml:lang="en-US">Specifies the name of the author for this entry</rdfs:comment>
    <rdfs:label xml:lang="en-US">Author Name</rdfs:label>
    <rdfs:domain rdf:resource="#HistoryRecord"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#structureType">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Graph</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Explicit_Slice</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Implicit_Slice</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:comment xml:lang="en-US">Specifies how this view is being constructed. &#xD;
&#xD;
Valid values are: Implicit Slice = a slice based on a filter criteria; Explicit Slice = a slice based on arbitrary membership, as defined by specific relationships between entries; Graph = a bounded graphical slice based on ChildOf relationships.</rdfs:comment>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:label xml:lang="en-US">View Structure</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#impact.confidentiality">
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies the typical impact of this pattern on the confidentiality characteristics of the targeted software and related data.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Confidentiality Impact</rdfs:label>
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab</edt:rangeControlledVocabSet>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab-1.0</edt:rangeControlledVocabClass>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandType">
    <rdfs:label xml:lang="en-US">Type</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the type of the command structure</rdfs:comment>
    <rdfs:domain rdf:resource="#CommandStructure"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#viewName">
    <rdfs:comment xml:lang="en-US">Specifies a descriptive attribute used to give the reader an idea of what perspective this view represents. All words in the name should be capitalized except for articles and prepositions unless they begin or end the name. Subsequent words in a hyphenated chain are also not capitalized.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">view name</rdfs:label>
    <rdfs:domain rdf:resource="#View"/>
  </owl:DatatypeProperty>

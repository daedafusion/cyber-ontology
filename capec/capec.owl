    <rdfs:label xml:lang="en-US">Attack Pattern Identifier</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:domain rdf:resource="#CompoundElement"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#referenceTitle">
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:comment xml:lang="en-US">Specifies the title of the material beingreferenced. It is not required if the material does not have a title.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Title</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="typicalLikelyhoodOfExploit.explanation">

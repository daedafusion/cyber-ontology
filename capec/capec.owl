          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Client-side</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:comment xml:lang="en-US">Specifies the localities, as "client-side" or "server-side", of the targeted attack surface</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#attackSurfaceId">
    <rdfs:domain rdf:resource="#CommonAttackSurfaceDescription"/>
    <rdfs:domain rdf:resource="#CommonAttackSurface"/>

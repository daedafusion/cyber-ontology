        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a short description</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#formattedIdentifier -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#formattedIdentifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the Common Attack Pattern Enumeration and Classification (CAPEC) identifier associated with the entity.  The identifier should follow the CAPEC identifier pattern of &quot;CAPEC-nnnn&quot; where &apos;nnnnn&apos; specifies an numerically incrementing number.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Attack Pattern Enumeration and Classification (CAPEC) Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://capec.mitre.org/capec#functionalServiceId -->


    <owl:DatatypeProperty rdf:about="http://capec.mitre.org/capec#functionalServiceId">

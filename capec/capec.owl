    <rdfs:label xml:lang="en-US">Technical Context</rdfs:label>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies the technical context where this pattern is applicable.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#referencePublisherName">
    <rdfs:comment xml:lang="en-US">Specifies the name of the publisher of the reference material, if one exists.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:label xml:lang="en-US">Publisher</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="backgroundDetails">
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:label xml:lang="en-US">Background Details</rdfs:label>
    <rdfs:comment xml:lang="en-US">Speicfies information regarding the entry or any technologies that are related to it, where the background information is 	not related to the nature of the entry itself. It should be filled out where appropriate.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#submissionSource">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>

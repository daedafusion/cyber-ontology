      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:comment xml:lang="en-US">Specifies how the initial information for this entry was obtained.</rdfs:comment>
    <rdfs:label xml:lang="en-US">submission source</rdfs:label>
    <rdfs:domain rdf:resource="#HistoryRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#securityControlId">
    <rdfs:label xml:lang="en-US">Security Control Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a unique integer identifier for the security control.</rdfs:comment>
    <rdfs:domain rdf:resource="#SecurityControl"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#environmentId">
    <rdfs:comment xml:lang="en-US">Specifies a globally unique Identifier for a kind of environment</rdfs:comment>
    <rdfs:label xml:lang="en-US">Environment Identifier</rdfs:label>
    <rdfs:domain rdf:resource="#Environment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#contributionSource">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"

                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >OWGV</rdf:first>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Educators</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Assessment_Vendors</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Software_Vendors</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Developers</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en-US">Stakeholder</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies what types of members of the Cyber Security community might be interested in this view.</rdfs:comment>
    <rdfs:domain rdf:resource="#Audience"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="codeExampleLanguage">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#CodeLanguageVocab-1.0</edt:rangeControlledVocabClass>
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#CodeLanguageVocab</edt:rangeControlledVocabSet>
    <rdfs:comment xml:lang="en-US">Specifies the programming language being used in the following block of Code</rdfs:comment>
    <rdfs:label xml:lang="en-US">Code Example Language</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#viewObjective">
    <rdfs:label xml:lang="en-US">View Objective</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the perspective from which this View is constructed.</rdfs:comment>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#techniqueName">
    <rdfs:domain rdf:resource="#ProbingTechnique"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Technique Name</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a name associated with the technique</rdfs:comment>
    <rdfs:domain rdf:resource="#ObfuscationTechnique"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandStructureName">
    <rdfs:label xml:lang="en-US">Name</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a name associated with the Command Structure</rdfs:comment>
    <rdfs:domain rdf:resource="#CommandStructure"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="payload">
    <rdfs:label xml:lang="en-US">Payload</rdfs:label>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies the code, configuration or other data to be executed or otherwise activated as part of an injection-based attack of this type.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#formattedIdentifier">
    <rdfs:comment xml:lang="en-US">Specifies the Common Attack Pattern Enumeration and Classification (CAPEC) identifier associated with the entity.  The identifier should follow the CAPEC identifier pattern of "CAPEC-nnnn" where 'nnnnn' specifies an numerically incrementing number.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:label xml:lang="en-US">Common Attack Pattern Enumeration and Classification (CAPEC) Identifier</rdfs:label>
    <rdfs:domain rdf:resource="#CompoundElement"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#securityControlType">
    <rdfs:label xml:lang="en-US">Security Control Type</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the type of the Security Control as “Detective,” “Corrective,” or “Preventative.” Detective controls detect an attacker’s activities in the attack step, whether the activities are successful or not. Corrective controls attempt to mitigate an attacker’s success by responding to a successful outcome. They are not related to or normalized against outcomes. Preventative controls are those that make the attack step unlikely or impossible to succeed.</rdfs:comment>
    <rdfs:domain rdf:resource="#SecurityControl"/>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Preventative</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Corrective</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Detective</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#phaseId">
    <rdfs:label xml:lang="en-US">Attack Phase Id</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the identifier of the attack phase</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPhase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#skillKnowledgeLevel">
    <rdfs:domain rdf:resource="#Knowlege"/>
    <rdfs:label xml:lang="en-US">Skill or Knowledge Level</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the level of skill or knowledge required by the attacker communicated on a rough scale.&#xD;
&#xD;
 For example: Low - Basic computer familiarity,  Low - Basic SQL knowledge ; &#xD;
                         Medium - Moderate scripting and shell experience and ability to disassemble and decompile;&#xD;
                         High - Expert knowledge of LINUX kernel, High - Detailed knowledge of target software development practices and business context (former employee)</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >High</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Medium</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Low</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#encryptionUsed">
    <rdfs:domain rdf:resource="#TargetedProtocol"/>
    <rdfs:comment xml:lang="en-US">Specifies whether the encryption was utilized</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label xml:lang="en-US">Encryption</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="typicalSeverity">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Very Low</rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Low</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Medium</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >High</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Very High</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab-1.0</edt:rangeControlledVocabClass>
    <rdfs:label xml:lang="en-US">Typical Severity</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies on a rough scale (Very Low, Low, Medium, High, Very high), what is the typical severity of impact to the targeted software if this attack occurs? The severity of a specific attack instance can vary greatly depending on the specific context of the target software under attack. This field is intended to capture an overall typical average value for this type of attack with the understanding that it will not be completely accurate for all attacks.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab</edt:rangeControlledVocabSet>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#indicatorId">
    <rdfs:label xml:lang="en-US">Indicator Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies an identifier for the Indicator</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#SusceptabiltyIndicator"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="imageTitle">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en-US">Specifies a brief title associated with the image</rdfs:comment>
    <rdfs:label xml:lang="en-US">Image Title</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#compoundElementAbstraction">
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:comment xml:lang="en-US">Specifies the abstraction level for this attack pattern. The abstraction levels for Compound Elements and Attack Patterns 	are the same. For example, if the Compound Element is a chain, and all elements of the chain are Meta level, then the Compound Element Abstraction attribute is Meta.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Compound Element Abstraction</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Meta</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Standard</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Detailed</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="technicalContext.languages">
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Languages</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies one or more languages in which this attack pattern is possible and relevant.  Languages characterizes the target using an enumerated list of languages utilized by the target.</rdfs:comment>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#CodeLanguageVocab-1.0</edt:rangeControlledVocabClass>
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#CodeLanguageVocab</edt:rangeControlledVocabSet>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="authorOganizationName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Author's Oganization Name</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the name of the author's organization</rdfs:comment>
    <rdfs:domain rdf:resource="#HistoryRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#consequenceNote">
    <rdfs:label xml:lang="en-US">Consequence Note</rdfs:label>
    <rdfs:domain rdf:resource="#Consequence"/>
    <rdfs:comment xml:lang="en-US">Specifies additional commentary about this consequence.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#compoundElementCompleteness">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Complete</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Stub</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Hook</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en-US">Compound Element Completeness</rdfs:label>
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:comment xml:lang="en-US">Specifies the completeness of the Compound Element</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="typicalLikelihoodOfExploit">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Typical Likelihood of Exploit</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies on a rough scale (Very Low, Low Medium,  High, Very High), what is the overall likelihood of this type of attack typically succeeding considering the attack prerequisites, targeted weakness attack surface, skill required and resources required as well as available and likely implemented blocking solutions? The likelihood of exploit of a specific attack instance can vary greatly depending on the specific context of the target software under attack. This field is intended to capture an overall typical average value for this type of attack with the understanding that it will not be completely accurate for all attacks.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab-1.0</edt:rangeControlledVocabClass>
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab</edt:rangeControlledVocabSet>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#impact.integrity">
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab</edt:rangeControlledVocabSet>
    <rdfs:label xml:lang="en-US">Integrity Impact</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies  the typical impact of this pattern on the integrity characteristics of the targeted software and related data.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#HighMediumLowVocab-1.0</edt:rangeControlledVocabClass>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#rfc">
    <rdfs:label xml:lang="en-US">RFC</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the Request For Comment (RFC) associated with the protocol</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#targetedOSILayers">
    <rdfs:domain rdf:resource="#TargetAttackSurfaceDescription"/>
    <rdfs:label xml:lang="en-US">Targeted Attack Surface OSI Layers</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the OSI Layers that are part of the targeted Attack Surface</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >Application Layer</rdf:first>
                    </rdf:rest>
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Presentation Layer</rdf:first>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Session Layer</rdf:first>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Transport Layer</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Network Layer</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Data Link Layer</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Physical Layer</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="keywords">
    <rdfs:comment xml:lang="en-US">Specifies one or more keywords.  Keyword correspond to text strings used to tag and search CAPEC catalog data.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:label xml:lang="en-US">keywords</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="securityControlDescription">
    <rdfs:label xml:lang="en-US">Description</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a brief description of the Security Control</rdfs:comment>
    <rdfs:domain rdf:resource="#SecurityControl"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#techniqueId">
    <rdfs:label xml:lang="en-US">Technique Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies an identifier for the technique</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#AttackStepTechnique"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="text">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en-US">Specifies a paragraph of text.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Text</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="code">
    <rdfs:label xml:lang="en-US">Code</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a line of code.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="methodsOfAttack">
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:comment xml:lang="en-US">Specifies the mechanism of attack used by this pattern. This field can help define the applicable attack surface required for this attack.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <edt:rangeControlledVocabSet rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#CodeLanguageVocab</edt:rangeControlledVocabSet>
    <rdfs:label xml:lang="en-US">Methods of Attack</rdfs:label>
    <edt:rangeControlledVocabClass rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.daedafusion.com/default_vocabularies-1#MethodsOfAttackVocab-1.0</edt:rangeControlledVocabClass>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="entryComment">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Comment</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies any comments regarding the content of this attack pattern entry, such as assumptions made, reasons for omitting elements, contact information, pending questions, etc.</rdfs:comment>
    <rdfs:domain rdf:resource="#HistoryRecord"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#dictionaryLink">
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:comment xml:lang="en-US">Specifies an external link to the specific attack pattern contained at the Common Attack Pattern Enumeration and Characterizaton (CAPEC) dictionary.  The link should be in the following form:  http://capec.mitre.org/data/definitions/{attack-pattern-id}.html, where {attack-pattern-id) should be the integer Identifier of the specific attack pattern.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:label xml:lang="en-US">Dictionary Link</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#relationshipViewOrdinal">
    <rdfs:comment xml:lang="en-US">Specifies if this relationship is the primary ChildOf relationship for this entry for a given RelationshipView element.. This attribute can only have the value "Primary" and should only be included for the primary parent/child relationship.</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Primary</rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en-US">Relationship View Ordinal</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#indicatorType">
    <rdfs:label xml:lang="en-US">Indicator Type</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a mandatory type that can be one of the values “Positive,” “Negative,” or “Inconclusive.” &#xD;
&#xD;
For example, a positive indicator of susceptibility to parameter tampering is the existence of parameters in the URL. Although it does not guarantee susceptibility, it indicates a cause for further examination. A negative indicator for the technique of privilege escalation is a lack of credentials and user identifiers in an application. Again, this is not a conclusive measure of resistance to attack, but an indicator that the attack step technique is unlikely to bear significant fruit. An inconclusive indicator of susceptibility to dynamic code injection is a page whose URL ends in .jsp, .asp, or .do but which has no visible explicit parameters. Such URLs typically indicate dynamic processing, but since no visible parameters are passed, it is inconclusive whether dynamic code could be injected into the application.</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Inconclusive</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Negative</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Positive</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:domain rdf:resource="#SusceptabiltyIndicator"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#extendedDescription">
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:domain rdf:resource="#IndicatorWarning"/>
    <rdfs:comment xml:lang="en-US">Specifies a short description</rdfs:comment>
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:domain rdf:resource="#ObfuscationTechnique"/>
    <rdfs:domain rdf:resource="#ProbingTechnique"/>
    <rdfs:domain rdf:resource="#Audience"/>
    <rdfs:range rdf:resource="#structuredText"/>
    <rdfs:label xml:lang="en-US">Description</rdfs:label>
    <rdfs:domain rdf:resource="#Term"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="solutionsAndMitigations">
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:label xml:lang="en-US">Solutions and Mitigations</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies one or more solutions or mitigations. A solution or mitigation describes actions or approaches to prevent or mitigate the risk of this attack by improving the resilience of the target system, reduce its attack surface or to reduce the impact of the attack if it is successful.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#term">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://capec.mitre.org/capec&gt;</rdfs:isDefinedBy>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >attackpattern</edt:namespacePrefix>
    <rdfs:domain rdf:resource="#Term"/>
    <rdfs:comment xml:lang="en-US">Specifies the actual term.  Each term should follow the same conventions as they entry Name attribute.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Term</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#vulnerabilityDescription">
    <rdfs:domain rdf:resource="#RelatedVulnerabilityStatement"/>
    <rdfs:label xml:lang="en-US">Vulnerability Description</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a short textual description of the specific related vulnerability taken from the industry standard vulnerability listing.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
    <rdfs:domain rdf:resource="#ExampleInstance"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#functionalServiceId">
    <rdfs:label xml:lang="en-US">Functional Service Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the identifier for the Funcational Service</rdfs:comment>
    <rdfs:domain rdf:resource="#FunctionalService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="purposes">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Obfuscation</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Exploitation</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Penetration</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Reconnaissance</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:label xml:lang="en-US">Purposes</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies one or more purposes.  Purpose refers to the intended purpose behind the attack pattern relative to an enumerated list of attack objectives. &#xD;
&#xD;
USAGE: This element is used to capture pattern composibility and assist with normalization and classification of attack patterns within the CAPEC catalog.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="relatedSecurityPrinciples">
    <rdfs:label xml:lang="en-US">Related Security Principles</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies one or more related security principles. A principle is defined as a rule or standard for good behavior. A related security principle is a security rule or practice that impedes this attack pattern. &#xD;
&#xD;
USAGE: Usage defined in NIST SP 800-27A, "Engineering Principles for Information Technology Security", Revision A.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="referenceAuthorName">
    <rdfs:comment xml:lang="en-US">Specifies the name of an individual author of the material being referenced. It is not required, but may be repeated sequentially in order to identify multiple authors for a single piece of material.</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="en-US">Reference Author</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="relevantSecurityPatterns">
    <rdfs:comment xml:lang="en-US">Specifies one or more relevant security patterns. A relevant security pattern provides resistance or resilience to this type of attack.</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:label xml:lang="en-US">relevant security patterns</rdfs:label>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#resourcesRequired">
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:label xml:lang="en-US">Resources Required</rdfs:label>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:comment xml:lang="en-US">Specifies the resources (CPU cycles, IP 	addresses, tools, etc.) required by an attacker to effectively execute this type of attack.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#consequenceId">
    <rdfs:label xml:lang="en-US">Consequence Identifier</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies the value for the related Common_Consequence entry identifier as a string.  Only one Common Consequence Identifier can exist for each Consequence element (ex: CC-1).  However, Consequences across CAPEC with the same ID should only vary in small details.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Consequence"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#relativeId">
    <rdfs:domain rdf:resource="#Outcome"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#HistoryRecord"/>
    <rdfs:comment xml:lang="en-US">Specifies an identifier for the related entity.  Only one relative ID element can exist for each entity (ex: R.78.1). Text citing this reference should use the format [R.78.1] where "R" is some identifier, "78" is the entity identifier and "1" is the ordinal value of the reference (e.g., 1 being the first, 2 being the second)</rdfs:comment>
    <rdfs:domain rdf:resource="#AttackPhase"/>
    <rdfs:domain rdf:resource="#TargetedProtocol"/>
    <rdfs:label xml:lang="en-US">relative id</rdfs:label>
    <rdfs:domain rdf:resource="#Consequence"/>
    <rdfs:domain rdf:resource="#ObfuscationTechnique"/>
    <rdfs:domain rdf:resource="#ProbingTechnique"/>
    <rdfs:domain rdf:resource="#AttackStepTechnique"/>
    <rdfs:domain rdf:resource="#SecurityControl"/>
    <rdfs:domain rdf:resource="#TargetAttackSurfaceDescription"/>
    <rdfs:domain rdf:resource="#SusceptabiltyIndicator"/>
    <rdfs:domain rdf:resource="#IndicatorWarning"/>
    <rdfs:domain rdf:resource="#AttackStep"/>
    <rdfs:domain rdf:resource="#ExampleInstance"/>
    <rdfs:domain rdf:resource="#CommonAttackSurfaceDescription"/>
    <rdfs:domain rdf:resource="#Knowlege"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandDescription">
    <rdfs:label xml:lang="en-US">Description</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies a brief description of the command structure</rdfs:comment>
    <rdfs:domain rdf:resource="#CommandStructure"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#vulnerabilityId">
    <rdfs:label xml:lang="en-US">Vulnerability Id</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en-US">Specifies the vulnerability identifier, such as Common Vulnerabilities and Explosures (CVE), Common Attack Pattern Enumeration and Classification (CAPEC), US-CERT number, or vendor-specific identifier that identifies the vulnerability.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#title">
    <rdfs:label xml:lang="en-US">Title</rdfs:label>
    <rdfs:domain rdf:resource="#AttackPattern"/>
    <rdfs:domain rdf:resource="#AttackStep"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en-US">Specifies a short descriptive title. It should be kept as short as possible but also clearly convey the nature of what is being described</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#targetProtocolId">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label xml:lang="en-US">Protocol Identifier</rdfs:label>
    <rdfs:domain rdf:resource="#TargetedProtocol"/>
    <rdfs:comment xml:lang="en-US">Specifies the identifier associated with the protocol being targeted</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="researchGaps">
    <rdfs:domain rdf:resource="#CompoundElement"/>
    <rdfs:domain rdf:resource="#View"/>
    <rdfs:domain rdf:resource="#Category"/>
    <rdfs:label xml:lang="en-US">Research Gaps</rdfs:label>
    <rdfs:comment xml:lang="en-US">Specifies zero or more Research gap elements, each of which identifies potential opportunities for the vulnerability research community to conduct further exploration of issues related to this attack pattern.  It is intended to highlight parts of CAPEC that have not received sufficient attention from researchers. This should be filled out where appropriate for attack patterns and categories.</rdfs:comment>
    <rdfs:range rdf:resource="#structuredText"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="technicalContext.architecturalParadigms">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">

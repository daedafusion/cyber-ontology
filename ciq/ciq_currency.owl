<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:xcl="http://www.oasis-open.org/ciq_currency#"
    
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.oasis-open.org/ciq_currency">
  <owl:Ontology rdf:about="http://www.oasis-open.org/ciq_currency">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1.0.0-000</owl:versionInfo>
    <owl:imports rdf:resource="http://www.oasis-open.org/ciq_common"/>
    <rdfs:comment >This ontology provides a vocabulary and representation of the OASIS Customer Information Quality (CIQ) eXtensible Currency Language.&#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    <rdfs:label >OASIS Customer Information Quality (CIQ) eXtensible Currency Language (xCL) Ontology</rdfs:label>
    
  </owl:Ontology>
  <owl:Class rdf:ID="Currency">
    <rdfs:label >Currency</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="isoCurrencyCode"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment >Characterizes currency</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="currencyName"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <rdf:Description rdf:about="http://www.oasis-open.org/ciq_common#id">
            <!-- <rdfs:domain rdf:resource="#Currency"/> -->
          </rdf:Description>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="isoNumericCurrencyCode"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="currencySymbol"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="#isoNumericCurrencyCode">
    <!-- <rdfs:domain rdf:resource="#Currency"/> -->
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >iso numeric currency code</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment >Specifies the three-digit numerica code per ISO 4217.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#currencyName">
    <rdfs:label >Currency Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the name of the currency</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#Currency"/> -->
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="currencySymbolCodes">
    <rdfs:label >Currency Symbol Codes</rdfs:label>
    
    <!-- <rdfs:domain rdf:resource="#Currency"/> -->
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the currency symbol as a sequence of one or more comma-separated unicode hex values</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#isoCurrencyCode">
    <rdfs:label >ISO 4217 Currency Code</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#Currency"/> -->
    
    <rdfs:comment >Specifies the 3-character currency code per ISO 4217</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#currencySymbol">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the symbol associated with the curency</rdfs:comment>
    <rdfs:label >currency symbol</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <!-- <rdfs:domain rdf:resource="#Currency"/> -->
  </owl:DatatypeProperty>
</rdf:RDF>


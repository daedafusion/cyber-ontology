        <owl:onProperty rdf:resource="http://www.oasis-open.org/ciq_common#dataQuality.validFromDateTime"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment >Characterizes details about an organization's name</rdfs:comment>

  <owl:ObjectProperty rdf:ID="organizationName.subdivision">
    <rdfs:comment >Specifies a sub-division of the organization</rdfs:comment>
    <rdfs:label >Organization Sub-Division</rdfs:label>
    <rdfs:range rdf:resource="#Subdivison"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.birthName">
    <rdfs:comment >Specifies the name given to an individual at birth, but later changed (common in some cultures).</rdfs:comment>
    <rdfs:label >Name at Birth</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="otherNames">
    <rdfs:comment >Specifies any other additional names that are not directly used to identify or call the individual, such as names of ancestors, saints, etc.</rdfs:comment>
    <rdfs:label >Other Name</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.knownAs">
    <rdfs:comment >Specifies names that the person is also known as</rdfs:comment>
    <rdfs:label >Known As</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.legalName">
    <rdfs:comment >Specifies the individual's legal name</rdfs:comment>
    <rdfs:label >Legal Name</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.formerName">
    <rdfs:label >Former Name</rdfs:label>
    <rdfs:comment >Specifies the former name of the individual</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.aliases">
    <rdfs:comment >Specifies other names by which the person is know.</rdfs:comment>
    <rdfs:label >Aliases</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.unofficialName">
    <rdfs:comment >Specifies an unofficial name that the individual is known to go by.</rdfs:comment>
    <rdfs:label >Unofficial Name</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.commonUse">
    <rdfs:comment >Specifies the name that is comonly used by others, e.g., a simplied form of the official name.</rdfs:comment>
    <rdfs:label >person name.Common use</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="nameDetails">
    <rdfs:comment >Specifies details about the names associated with the party</rdfs:comment>
    <rdfs:label >Name Details</rdfs:label>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#OrganizationName"/>
          <owl:Class rdf:about="#PersonName"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:isDefinedBy rdf:resource=""/>
    
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.MaidenName">
    <rdfs:comment >Specifies the name of the individual before marriage.</rdfs:comment>
    <rdfs:label >Maiden Name</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="personName.officialName">
    <rdfs:comment >Specifies the official name of the person, e.g., as in the passport, incorporation certificate, etc.</rdfs:comment>
    <rdfs:label >Official Name</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.businessUnit">
    <rdfs:comment >Specifies the name of a business unit within an organization</rdfs:comment>
    <rdfs:label >Organization Business Unit Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationNameUsage">
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    <rdfs:comment >Specifies how an organization's name can be used and for what purposes.   It provides an indication of which name is a set of names to select for a given purpose. (e..g, used for legal purposes)</rdfs:comment>
    <rdfs:label >Organization Name Usage</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivision.divisionName">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >subdivision.division name</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    
    <rdfs:comment >Specifies the division name of an organization's subdivision</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personName.lastName">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Last Name</rdfs:label>
    <rdfs:comment >Specifies the name elemnt that identifies the group the individual belongs to and is identified by, such as last name, surname, family name, etc.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personName.precedingTitle">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Preceding Title</rdfs:label>
    <rdfs:comment >Specifies the preceding title of a person (e.g., His Excellency, Honorable, etc.)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personName.generationIdentifier">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Sr.</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Jr.</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >II</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >III</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >IV</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >V</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >VI</rdf:first>
                      <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >VII</rdf:first>
                        <rdf:rest rdf:parseType="Resource">
                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >VIII</rdf:first>
                          <rdf:rest rdf:parseType="Resource">
                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >IX</rdf:first>
                            <rdf:rest rdf:parseType="Resource">
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >X</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:rest>
                          </rdf:rest>
                        </rdf:rest>
                      </rdf:rest>
                    </rdf:rest>
                  </rdf:rest>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:comment >Specifies a generational identifier (e.g., Jr, Sr, 2nd, IV, etc.)</rdfs:comment>
    <rdfs:label >Generation Identifier</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.orgType">
    <rdfs:label >organization name.org type</rdfs:label>
    <rdfs:comment >Specifies the type of the organization.  "Pty. Ltd" in Sakthisoft Pty.Ltd, where "Sakthisoft" is the name of the organisation, "Inc" in ABC Inc, where "ABC" is organisation name.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personId">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Person ID</rdfs:label>
    <rdfs:comment >Specifies a unique identifier of a person</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personName.firstName">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:comment >Specifies the most important name element by which this particular individual is identified in the group (e.g., John, Sam, Brian for Anglo-Saxon cultures)</rdfs:comment>
    <rdfs:label >First Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="partyName.jointNameConnector">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >AND</rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:comment >Specifies the connector used to join more than one person name.&#xD;
&#xD;
E xample: Mr Hunt AND Mrs Clark, where AND is the JointNameConnector.  The flow is from the preceding to the following. If there is more than 2 names then all names are connected using this connector in the natural order.</rdfs:comment>
    <rdfs:label >party name.Joint name connector</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personName.middleName">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Middle Name</rdfs:label>
    <rdfs:comment >Specifies the name element related to additional identification of the individual, such names are parents or places</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personNameUsage">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Person Name Usage</rdfs:label>
    <rdfs:comment >Specifies how a person's name can be used and for what purposes.  It provides an indication of which name is a set of names to select for a given purpose. (e..g, used for legal purposes)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="partyNameType">
    <rdfs:comment >Specifies the type of a Party Name ID</rdfs:comment>
    <rdfs:label >Party Name ID Type</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Person</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Organization</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Unknown</rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personName.preferredName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Preferred Name</rdfs:label>
    <rdfs:comment >Specifies the name that the individual prefers to be called by, e.g., "Jim" instead of "James", "Bill" instead of "William"</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivision.departmentName">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >subdivision.department name</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    
    <rdfs:comment >Specifies the department name of an organization's subdivision</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="nameLineType">
    <rdfs:comment >Specifies what the free form name line means.  For example, the type could be "Unknown"</rdfs:comment>
    <rdfs:label >Name Line Type</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivision.schoolName">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >subdivision.school name</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    
    <rdfs:comment >Specifies the school name of an organization's subdivision</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.department">
    <rdfs:label >Organization Department Name</rdfs:label>
    <rdfs:comment >Specifies the name of the department within an organization.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="partyName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Name of Party</rdfs:label>
    <rdfs:comment >Specifies the name of a party when it is unknown whether the party is an person or an organziation.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="partyNameUsage">
    <rdfs:label >Party Name Usage</rdfs:label>
    <rdfs:comment >Specifies how a party's name can be used and for what purposes (e.g., data exchange, contact, update, create).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.nameOnly">
    <rdfs:comment >Specifies just the name of the organization.  For example, in "Sakthisoft Pty. Ltd", "Pty. Ltd" is the legal entity for the organization name "Sakthisoft"</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Organization Name</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivision.branchName">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the branch name of an organization's subdivision</rdfs:comment>
    <rdfs:label >SubDivision Branch Name</rdfs:label>
    
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personName.title">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Mr.</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Mrs.</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Ms.</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Miss</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Professor</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Dr.</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >Sir</rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:rest>
                  </rdf:rest>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:comment >Specifies a title of some sorts, such as Mr, Miss, Ms (marriage status), or eduction such as Professor, Phd, Dr, etc.</rdfs:comment>
    <rdfs:label >Title</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personIdType">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Person ID Type</rdfs:label>
    <rdfs:comment >Specifies a type of person ID</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#organizationName.fullName">
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Full Name</rdfs:label>
    <rdfs:comment >Specifies the full name of an organization, including its name, propriety type, liability type, etc.(e.g., Sakthisoft Pty. Ltd)</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personName.nickname">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:label >Nickname</rdfs:label>
    <rdfs:comment >Specifies a simple nick name that is commonly used as part of the name (e.g., a fancy kick-boxer can be commonly known as Bill "Storm" Bababoons, where "Storm" is obviously an alias)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.division">
    <rdfs:comment >Specifies the name of the division within an organization.</rdfs:comment>
    <rdfs:label >Organization Division</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivisionName">
    <!-- <rdfs:domain rdf:resource="#Subdivison"/> -->
    <rdfs:comment >Specifies the name of a subdivision of an organziation (e.g. department, branch, division, section)</rdfs:comment>
    <rdfs:label >Subdivision name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="partyNameId">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Party Name ID</rdfs:label>
    <rdfs:comment >Specifies a unique identifier for a party</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.branch">
    <rdfs:comment >Specifies the name of a branch within an organization</rdfs:comment>
    <rdfs:label >Organization Branch Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#personNameType">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:comment >Specifies common types of person names</rdfs:comment>
    <rdfs:label >Person Name Type</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Alias</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >LegalName</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >KnownAs</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >MaidenName</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >FormerName</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >CommonUse</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >NameAtBirth</rdf:first>
                      <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >OfficialName</rdf:first>
                        <rdf:rest rdf:parseType="Resource">
                          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >UnofficialName</rdf:first>
                          <rdf:rest rdf:parseType="Resource">
                            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >NickName</rdf:first>
                            <rdf:rest rdf:parseType="Resource">
                              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                              >PetName</rdf:first>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:rest>
                          </rdf:rest>
                        </rdf:rest>
                      </rdf:rest>
                    </rdf:rest>
                  </rdf:rest>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivision.businessUnitName">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >subdivision.business unit name</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    
    <rdfs:comment >Specifies the business unit name of an organization's subdivision</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.school">
    <rdfs:comment >Specifies the name of a school within an organization</rdfs:comment>
    <rdfs:label >Organization School Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#organizationNameType">
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    <rdfs:comment >Specifies just the type of an organization without its name. For example, in "Sakthisoft Pty. Ltd", "Pty. Ltd" is the legal entity  or type for the organization name "Sakthisoft", "Inc." in "ABC, Inc." "Inc." is the type and "ABC" is the organization name</rdfs:comment>
    <rdfs:label >Type</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >LegalName</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >FormerName</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >CommonUse</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >PublishingName</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >OfficialName</rdf:first>
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >UnofficialName</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >Undefined</rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:rest>
                  </rdf:rest>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="jointNameConnector">
    <rdfs:comment >Specifies the connector used to join more than one person name.  Example: Mr. Hunt AND Mrs Clark, where AND is the joint name connector.  If there is more than 2 names, then all names are connected using the this connector in the natural order.</rdfs:comment>
    <rdfs:label >Joint Name Connector</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subDivisionType">
    <!-- <rdfs:domain rdf:resource="#Subdivison"/> -->
    <rdfs:comment >Specifies a type of subdivision of an organization</rdfs:comment>
    <rdfs:label >Subdivision Type</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Section</rdf:first>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >School</rdf:first>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >BusinessUnit</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Branch</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Division</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >Department</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personName.petName">
    <rdfs:comment >Specifies a "pet name" by which the person is known to individuals that have a close relationship with the person.</rdfs:comment>
    <rdfs:label >Pet Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="nameLine">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Name</rdfs:label>
    <rdfs:comment >Specifies a name as a free-form text.  Use this when the type of the entity (person or organization) is unknown or is not broken down into individual elements (e.g., unstructured, unparsed) or is beyond the provided types.  The name represented may be formatted in the righ order or may not be as it is not parsed/broken into atomic fields</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationName.section">
    <rdfs:comment >Specifies the name of a section within an organization.</rdfs:comment>
    <rdfs:label >Organization Section Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subdivision.sectionName">
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >subdivision.section name</rdfs:label>
    <!-- <rdfs:domain rdf:resource="#OrganizationName"/> -->
    
    <rdfs:comment >Specifies the section name of an organization's subdivision</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="organizationIdType">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the type of an organization ID</rdfs:comment>
    <rdfs:label >Organization ID Type</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personName.otherName">
    <rdfs:label >Other Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies any other additional names that are not directly used to identify or call the individual, such as names of ancestors, saints, etc.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="personName.credentials">
    <!-- <rdfs:domain rdf:resource="#PersonName"/> -->
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies an educational degree or credentials (e.g., PhD)</rdfs:comment>
    <rdfs:label >Credentials</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>


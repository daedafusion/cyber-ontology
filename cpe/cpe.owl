  xml:base="http://cpe.mitre.org/cpe">
  <owl:Ontology rdf:about="http://cpe.mitre.org/cpe">
    <rdfs:comment xml:lang="en-US">Ontology that provides a vocabulary and representation for the Common Platform Enumeration (CPE) which provides a standardized method of describing and identifying classes of applications, operating systems, and hardware devices present among an enterprise's computing assets.&#xD;
&#xD;
The ontology is based on the Common Platform Enumeration schema maintained by NIST copyright 2011-2014.  The version number of the ontology is kept in sync with the schema in order to more easily relate between the ontology and the corresponding schema version.&#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    <owl:imports rdf:resource="http://cybox.mitre.org/objects/System"/>
    <owl:imports rdf:resource="http://cybox.mitre.org/cyboxCommon"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2.3-000</owl:versionInfo>
    <owl:imports rdf:resource="http://scap.nist.gov/scap_core"/>
    <owl:imports rdf:resource="http://cybox.mitre.org/objects/Device"/>
    <owl:imports rdf:resource="http://www.daedafusion.com/editor_annotation"/>
    <owl:imports rdf:resource="http://cybox.mitre.org/objects/Product"/>
    <rdfs:label xml:lang="en-US">Common Platform Enumeration (CPE) Ontology</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:ID="ChangeDescription">
    <rdfs:comment xml:lang="en-US">Specifies a description of any changes made to the identifier name or associated metadata.</rdfs:comment>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >platform</edt:namespacePrefix>
    <rdfs:label xml:lang="en-US">Change Description</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="changeType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="evidenceType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:iriTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >cpe://Change/${changeDate}/${changeType}/@hash(${comment})</edt:iriTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="changeDate"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >${changeDate} ${changeType} : ${comment}</edt:displayNameTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="evidenceExternalLink"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

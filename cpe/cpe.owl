        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >${deprecationDate} ${deprecationType}</edt:displayNameTemplate>
    <rdfs:comment xml:lang="en-US">Specifies the deprecation information for a specific deprecation of a given identifier name.  If a name is deprecated multiple times, there would be a separate instance of the deprecation element for each deprecation.</rdfs:comment>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >platform</edt:namespacePrefix>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:Class>

		<rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
		<rdfs:range rdf:resource="http://cpe.mitre.org/cpe#CPE"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&cve;vulnerabilities">
		<rdfs:label>Vulnerabilities</rdfs:label>

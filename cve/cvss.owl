<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:cvss="http://nvd.nist.gov/cvss#"
    xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:edt="http://www.daedafusion.com/editor_annotation#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://nvd.nist.gov/cvss">
  <owl:Ontology rdf:about="http://nvd.nist.gov/cvss">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2.0.0-001</owl:versionInfo>
    <rdfs:comment xml:lang="en-US">This ontology provides a vocabulary and representation for the Common Vulnerability Scoring System (CVSS) v2 which provides an open framework for communicating the characteristics and impacts of IT vulnerabilities.  CVSS consists of three groups: Base, Temporal, and Environmental. &#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    <owl:imports rdf:resource="http://www.daedafusion.com/editor_annotation"/>
    <rdfs:label xml:lang="en-US">Common Vulnerability Scoring System (CVSS) Ontology</rdfs:label>
  </owl:Ontology>
  <owl:Class rdf:ID="TemporalMetrics">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="temporalMultiplier"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >(${score}) - E:${exportability}/RL:${remediationLevel}/RC:${reportConfidence}</edt:displayNameTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="exploitability"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="generationTime"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en-US">Defines the characteristics of a vulnerability that change over time but not among user environments.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="score"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="vector"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="displayLabel"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:iriTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >nvd:/CVSS/Temporal/${generationDate}/${score}-E:${exportability}/RL:${remediationLevel}/RC:${reportConfidence}</edt:iriTemplate>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="reportConfidence"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="remediationLevel"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">CVSS Temporal Metrics</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="EnvironmentalMetrics">
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#score"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:iriTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >nvd:/CVSS/Environ/${generationDate}/${score}-CDP:${collateralDamangePotential}/TD:${targetDistribution}/CR:${confidentialityRequirement}/IR:${integrityRequirement}/AR:${availabilityRequirement}</edt:iriTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="confidentialityRequirement"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="targetDistribution"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="availabilityRequirement"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en-US">Defines the characteristics of a vulnerability that are relevant and unique to a particular user's environment.</rdfs:comment>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >(${score} - CDP:${collateralDamangePotential}/TD:${targetDistribution}/CR:${confidentialityRequirement}/IR:${integrityRequirement}/AR:${availabilityRequirement}</edt:displayNameTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="integrityRequirement"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="collateralDamagePotential"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en-US">CVSS Environmental Metrics</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#generationTime"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#vector"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#displayLabel"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="BaseMetrics">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="impactSubscore"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >(${score}) - AV:${accessVector}/AC:${accessComplexity}/Au:${authenticationAttempts}/C:${confidentialityImpact}/I:${integrityImpact}/A:${availabilityImpact}</edt:displayNameTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#displayLabel"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="exploitSubscore"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#vector"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="confidentialityImpact"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="accessComplexity"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="availabilityImpact"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en-US">Defines the intrinsic and fundamental characteristics of a vulnerability that are constant with time and across user environments</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#generationTime"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">CVSS Base Metrics</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#score"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="integrityImpact"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="accessVector"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:iriTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >nvd:/CVSS/Base/${generationDate}/${score}-AV:${accessVector}/AC:${accessComplexity}/Au:${authenticationAttempts}/C:${confidentialityImpact}/I:${integrityImpact}/A:${availabilityImpact}</edt:iriTemplate>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="authenticationAttempts"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CVSSImpact">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="CVSS"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="overallScore"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="baseMetrics"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="baseScore"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="baseVector"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="temporalVector"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en-US">Defines Common Vulnerability Score System for a vulnerability.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="environmentalMetrics"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="environmentalVector"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">CVSS Impact</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#vector"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:iriTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >${iriPrefix}:/${cve-id}/score</edt:iriTemplate>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >${vector} Base: ${baseMetrics.score} Environ: ${environmentalMetric.score} Temporal: {$temporalMetrics.score}</edt:displayNameTemplate>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="environmentalScore"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="termporalMetrics"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="temporalScore"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#CVSS">
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >${vector} Base: ${baseMetrics.score} Environ: ${environmentalMetric.score} Temporal: {$temporalMetrics.score}</edt:displayNameTemplate>
    <rdfs:comment xml:lang="en-US">Defines Common Vulnerability Score System for a vulnerability</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#vector"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">CVSS</rdfs:label>
    <edt:iriTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >${iriPrefix}:/${cve-id}/score</edt:iriTemplate>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#termporalMetrics">
    <rdfs:range rdf:resource="#TemporalMetrics"/>
    <rdfs:comment xml:lang="en-US">Specifies a reference to the details for the Temporal Metrics</rdfs:comment>
    <rdfs:domain rdf:resource="#CVSS"/>
    <rdfs:label xml:lang="en-US">Temporal Metrics Details</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#environmentalMetrics">
    <rdfs:domain rdf:resource="#CVSS"/>
    <rdfs:label xml:lang="en-US">Environmental Metrics Details</rdfs:label>
    <rdfs:range rdf:resource="#EnvironmentalMetrics"/>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment xml:lang="en-US">Specifies a reference to the details for environmental metrics</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#baseMetrics">
    <rdfs:domain rdf:resource="#CVSS"/>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >vulnerability</edt:namespacePrefix>
    <rdfs:range rdf:resource="#BaseMetrics"/>
    <rdfs:comment xml:lang="en-US">Specifies a reference to the details for the base metrics</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label xml:lang="en-US">Based Metrics Details</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#integrityRequirement">
    <rdfs:comment xml:lang="en-US">Specifies the importance of the affected IT asset to a user's organization in terms of integrity.</rdfs:comment>
    <rdfs:label xml:lang="en-US">Integrity Requirement (IR)</rdfs:label>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >LOW</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >MEDIUM</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >HIGH</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >NOT_DEFINED</rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:domain rdf:resource="#EnvironmentalMetrics"/>
    <rdfs:isDefinedBy rdf:resource=""/>

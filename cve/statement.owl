<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:statement="http://nvd.nist.gov/statement#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://nvd.nist.gov/statement">
  <owl:Ontology rdf:about="http://nvd.nist.gov/statement">
    <owl:imports rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol"/>
    <rdfs:label >Common Vulnerability Enumeration (CVE) Vendor Statements Ontology</rdfs:label>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >1.0.0-000</owl:versionInfo>
    <rdfs:comment >This ontology provides a vocabulary and representation for the Common Vulnerability Enumeration (CVE) Vendor Statements which provided by vendors regarding a particular flaw affecting within a product.

Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    
    <owl:imports rdf:resource="http://nvd.nist.gov/cve"/>
    <owl:imports rdf:resource="http://www.oasis-open.org/ciq_xpil"/>
    <owl:imports rdf:resource="http://scap.nist.gov/scap_core"/>
  </owl:Ontology>
  <owl:Class rdf:ID="VendorStatement">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="contributorName"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="contributor"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="organizationName"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:comment >Characterizes statements provided by vendors regarding a particular flaw affecting within a product</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="vulnerabiltyName"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="lastModifiedDateTime"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label >Vendor Statement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="organization"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="vulnerability"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="description"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VendorStatementCatalog">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#generator"/>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#catalogVersion"/>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label >Vendor Statement Catalog</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#id"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://scap.nist.gov/scap_core#Catalog"/>
    <rdfs:comment >Characterizes a collection of vendor vulnerability statements into a Vendor Statement Catalog</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#catalogName"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#catalogDate"/>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#description"/>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="#organization">
    <rdfs:comment >Specifies a referene to the organization</rdfs:comment>
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:label >organization</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.oasis-open.org/ciq_xpil#Organization"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#vulnerability">
    <rdfs:label >Vulnerability</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:comment >Speceifies a reference to the assoicated vulnerability</rdfs:comment>
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:range rdf:resource="http://nvd.nist.gov/cve#Vulnerability"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#contributor">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Person"/>
          <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Organization"/>
          <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Party"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >Contributor</rdfs:label>
    <rdfs:comment >Specifies a reference to the contributor</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="statements">
    <rdfs:label >Statements</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="#VendorStatement"/>
    <rdfs:domain rdf:resource="#VendorStatementCatalog"/>
    <rdfs:comment >Specifies one or more vendor vulnerability statements</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#contributorName">
    <rdfs:comment >Specifies the name of the contributor</rdfs:comment>
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Contributor Name</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#lastModifiedDateTime">
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >Last Modified</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment >Specifies the date/time the statement was last modified.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#description">
    <rdfs:comment >Specifies a short description</rdfs:comment>
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Description</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#vulnerabiltyName">
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:label >vulnerabilty name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the identifier associated with the vulnerability</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#organizationName">
    <rdfs:comment >Specifies the name of the organization</rdfs:comment>
    
    <rdfs:domain rdf:resource="#VendorStatement"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Organization Name</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>


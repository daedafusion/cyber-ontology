<?xml version="1.0"?>
<rdf:RDF
    xmlns:cwe="http://cwe.mitre.org/cwe#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:capec="http://capec.mitre.org/capec#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:cve="http://nvd.nist.gov/cve#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://cwe.mitre.org/cwe">
  <owl:Ontology rdf:about="http://cwe.mitre.org/cwe">
    <owl:imports rdf:resource="http://capec.mitre.org/capec"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >v1.0.0-001</owl:versionInfo>
    <owl:imports rdf:resource="http://scap.nist.gov/scap_core"/>
    <owl:imports rdf:resource="http://cpe.mitre.org/cpe"/>
    
    <rdfs:label xml:lang="en-US">Common Weakness Enumeration (CWE) Ontology</rdfs:label>
    <owl:imports rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol"/>
    <owl:imports rdf:resource="http://nvd.nist.gov/cve"/>
    <rdfs:comment xml:lang="en-US">This ontology provides a vocabulary and representation for the Common Weakness Enumeration (CWE) which provides a unified, measurable set of software weaknesses that is enabling more effective discussion, description, selection, and use of software security tools and services that can find these weaknesses in source code and operational systems as well as better understanding and management of software weaknesses related to architecture and design. &#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="Audience">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="stakeholder"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">Audience</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    
    <rdfs:comment xml:lang="en-US">Characterizes a target audience or group for this view.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="stakeholderDescription"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    
  </owl:Class>
  <owl:Class rdf:ID="IntroductionMode">
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="text"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Class rdf:ID="StructuredText"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">Introduction Mode</rdfs:label>
    <rdfs:comment xml:lang="en-US">Defines the mode in which the weakness can be introduced.</rdfs:comment>
    
  </owl:Class>
  <owl:Class rdf:ID="Block">
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#StructuredText"/>
    </rdfs:subClassOf>
    
    <rdfs:label xml:lang="en-US">Block</rdfs:label>
    <rdfs:comment xml:lang="en-US">Defines a  Block of structured text</rdfs:comment>
    
  </owl:Class>
  <owl:Class rdf:ID="WeaknessCatalog">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#id"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#catalogDate"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#catalogVersion"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf rdf:resource="http://scap.nist.gov/scap_core#Catalog"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#catalogName"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">Weakness Catalog</rdfs:label>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#description"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://scap.nist.gov/scap_core#generator"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en-US">Characterizes a collection of software security issues (flaws, faults, bugs, vulnerabilities, weaknesses, etc).  The name used by CWE is usually "CWE".  However, if this collection is a subset of CWE, then a more appropriate name should be used.</rdfs:comment>
    
  </owl:Class>
  <owl:Class rdf:ID="CompoundElement">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="extendedDescription"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="formattedIdentifier"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="compoundElementName"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en-US">Characterizes a meaningful aggregation of several weaknesses, as in a chain like CWE-690: CWE-252 Unchecked Return Value can result in CWE-476 NULL Pointer Dereference or as in a composite like CWE-352 Cross-Site Request Forgery.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="status"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="causalNature"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="id"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">CompoundElement</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="compoundElementStructure"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="likelihood"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="descriptionSummary"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="abstraction"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="WeaknessOrdinality">
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="ordinality"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en-US">Weakness Ordinality</rdfs:label>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="ordinalityDescription"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en-US">Characterizes a Weakness Ordinality and indicates when this entry is primary - where the weakness exists independent of other weaknesses, or when this entry might be resultant - where the weakness is typically related to the presence of some other weaknesses.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="TechnologyClass">
    <rdfs:comment xml:lang="en-US">Characterizes a class of technology in which this entry may exist and with what frequency in the specified class.</rdfs:comment>
    
    
    <rdfs:isDefinedBy rdf:resource=""/>

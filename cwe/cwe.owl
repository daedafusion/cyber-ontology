<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY core "tag:champtc:core#">
	<!ENTITY cwe "http://cwe.mitre.org/cwe#">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xmlns="http://cwe.mitre.org/cwe#"
	xmlns:core="tag:champtc:core#"
	xmlns:cwe="http://cwe.mitre.org/cwe#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="http://cwe.mitre.org/cwe">
		<rdfs:label>Common Weakness Enumeration (CWE) Ontology</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">This ontology provides a vocabulary and representation for the Common Weakness Enumeration (CWE) which provides a unified, measurable set of software weaknesses that is enabling more effective discussion, description, selection, and use of software security tools and services that can find these weaknesses in source code and operational systems as well as better understanding and management of software weaknesses related to architecture and design. 

Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
		<owl:imports rdf:resource="http://capec.mitre.org/capec"/>
		<owl:imports rdf:resource="http://cpe.mitre.org/cpe"/>
		<owl:imports rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol"/>
		<owl:imports rdf:resource="http://nvd.nist.gov/cve"/>
		<owl:imports rdf:resource="http://scap.nist.gov/scap_core"/>
		<owl:versionInfo rdf:datatype="&xsd;string">v1.0.0-001</owl:versionInfo>
	</owl:Ontology>
	
	<owl:Class rdf:about="&cwe;ArchitectureParadigm">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;paradigmName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;prevalence"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Architecture Paradigm</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a single Architectural Paradigm in which this entry may exist and with what frequency in the specified paradigm.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Audience">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;stakeholder"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;stakeholderDescription"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Audience</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a target audience or group for this view.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;BackgroundDetail">
		<rdfs:subClassOf rdf:resource="&cwe;StructuredText"/>
		<rdfs:label>Background Detail</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Defines background information regarding the entry or any technologies that are related to it, where the background information is not related to the nature of the weakness itself. It should be filled out where appropriate.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;BlackBoxDefinition">
		<rdfs:subClassOf rdf:resource="&cwe;StructuredText"/>
		<rdfs:label>Black Box Definition</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Defines one or more Black Box Definitions, each of which describes the weakness from an external perspective, 	meaning that the view includes no knowledge of how the software is processing data other than what can be inferred from observing the software&apos;s behavior.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Block">
		<rdfs:subClassOf rdf:resource="&cwe;StructuredText"/>
		<rdfs:label>Block</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Defines a  Block of structured text</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Category">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;categoryName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;descriptionSummary"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;extendedDescription"/>
				<owl:someValuesFrom rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;formattedIdentifier"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;id"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;likelihood"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;status"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;timeOfIntroduction"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;weaknessName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Weakness Category</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a category entity that represents a collection of weaknesses based on a common attribute, such as CWE-310 Cryptographic Issues or CWE-355 User Interface Security Issues.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;CompoundElement">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;abstraction"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;causalNature"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;compoundElementName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;compoundElementStructure"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;descriptionSummary"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;extendedDescription"/>
				<owl:someValuesFrom rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;formattedIdentifier"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;id"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;likelihood"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;status"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>CompoundElement</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a meaningful aggregation of several weaknesses, as in a chain like CWE-690: CWE-252 Unchecked Return Value can result in CWE-476 NULL Pointer Dereference or as in a composite like CWE-352 Cross-Site Request Forgery.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Consequence">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;consequenceId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;consequenceNote"/>
				<owl:someValuesFrom rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;relativeId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Consequence</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a consequence that may be associated with the Weakness, Category, View, or Compound Element.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;DemonstrativeExample">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;demoExampleId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;exampleBody"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;introText"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;relativeId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Demonstrative Example</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a Demonstrative Example which should contain an example illustrating the problem being described in this weakness.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;DetectionMethod">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;detectionMethodId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;methodName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Detection Method</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes  information on different techniques that can be used to detect a weakness, including their strengths and limitations.  This should be filled out for some weakness classes and bases.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Environment">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;environmentName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;prevalence"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Environment</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a single Environment in which this entry may exist and with what frequency in the specified environment.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;ExploitationFactor">
		<rdfs:subClassOf rdf:resource="&cwe;StructuredText"/>
		<rdfs:label>Exploitation Factor</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Defines a condition or factor tha could increase the likelihood of exploit for this weakness.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;HardwareArchitecture">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;hardwareArchitectureClassDescription"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;hardwareArchitectureName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;prevalence"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Hardware Architecture</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a hardware achitecture on which this entry may exist and with what frequency on the specified architecture.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;HistoryRecord">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;authorName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;authorOrganization"/>
				<owl:someValuesFrom rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;authorOrganizationName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;contributionMode"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;entryDate"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;entryType"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;modificationImportance"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;modificationSource"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;relativeId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;submissionSource"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Content History</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a record of history to keep track of the author of the weakness entry and anyone who has made modifications to the content. This provides a means of contacting the authors and modifiers for clarifying ambiguities,  merging overlapping contributions, etc. This should be filled out for all entries.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;IntroductionMode">
		<rdfs:subClassOf rdf:resource="&cwe;StructuredText"/>
		<rdfs:label>Introduction Mode</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Defines the mode in which the weakness can be introduced.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Mitigation">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;mitigationEffectiveness"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;mitigationId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;relativeId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Mitigation</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes a method for mitigating a weakness.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;Note">
		<rdfs:subClassOf rdf:resource="&cwe;ResearchGap"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;noteType"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Note</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Defines one or more Relationship Note elements, which each contain details regarding the relationships between CWE entries.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;ObservedExample">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;relativeId"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Observed Example</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes an observed instance of this weakness in the real-world; typically this will be a CVE reference.  If the reference identifier is for a CVE reference, the cveReference should also be completed. This element should be filled out for as many entries as possible.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
		<core:appIconId rdf:datatype="&xsd;string">dl:com.champtc.champ.ui.core.stix.observedData</core:appIconId>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;OperatingSystem">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;osClass"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;osName"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cwe;prevalence"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Operating System</rdfs:label>
		<rdfs:comment rdf:datatype="&xsd;string">Characterizes an Operating System in which this entry may exist and with what frequency on the specified OS.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
	</owl:Class>
	
	<owl:Class rdf:about="&cwe;ProgrammingLanguage">

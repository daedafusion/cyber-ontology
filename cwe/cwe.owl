<?xml version="1.0"?>
<rdf:RDF xmlns="http://cwe.mitre.org/cwe#"
     xml:base="http://cwe.mitre.org/cwe"
	 xmlns:cwe="http://cwe.mitre.org/cwe#"
     xmlns:core="tag:champtc:core#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://cwe.mitre.org/cwe">
        <owl:imports rdf:resource="http://cpe.mitre.org/cpe"/>
        <owl:imports rdf:resource="http://scap.nist.gov/scap_core"/>
        <owl:imports rdf:resource="http://capec.mitre.org/capec"/>
        <owl:imports rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol"/>
        <owl:imports rdf:resource="http://nvd.nist.gov/cve"/>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">v1.0.0-001</owl:versionInfo>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This ontology provides a vocabulary and representation for the Common Weakness Enumeration (CWE) which provides a unified, measurable set of software weaknesses that is enabling more effective discussion, description, selection, and use of software security tools and services that can find these weaknesses in source code and operational systems as well as better understanding and management of software weaknesses related to architecture and design. 

Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Weakness Enumeration (CWE) Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- tag:champtc:core#appIconId -->


    <owl:AnnotationProperty rdf:about="tag:champtc:core#appIconId"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://cwe.mitre.org/cwe#alternateTerms -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#alternateTerms">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Term"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more  alternate terms by which this weakness may be known and a description to explain the context in which the term may be relevant</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Alternate Terms</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicableLanguages -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicableLanguages">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ProgrammingLanguage"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Languages which each represent a language in which this mitigation may exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Languages</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.ArchitectureParadigms -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.ArchitectureParadigms">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ArchitectureParadigm"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Architectural Paradigms  represent an architecture on which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Architecture Paradigms</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.CommonPlatformReferences -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.CommonPlatformReferences">
        <rdfs:range rdf:resource="http://cpe.mitre.org/cpe#CPE"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines references to the Common Platform Enumeration, CPE, which will identify common platforms on which this weakness may occur. It has one or more Common_Platform elements as children and each child will point to a single CPE entry which is associated with this weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Platform References</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.Environments -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.Environments">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Environment"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Environments which each represent an environment in which this weakness may exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Environments</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.HardwareArchitecture -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.HardwareArchitecture">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#HardwareArchitecture"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Hardware Architectures which each represent an architecture in which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Hardware Architectures</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.Languages -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.Languages">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ProgrammingLanguage"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a contains one or more Language elements which each represent a language in which this weakness may exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Programming Languages</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.OperatingSystems -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.OperatingSystems">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#OperatingSystem"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Operating System in which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Operating Systems</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.PlatformNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.PlatformNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a additional information related to the applicable platforms of a weakness if needed.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Platform Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#applicablePlatforms.TechnologyClasses -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#applicablePlatforms.TechnologyClasses">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#TechnologyClass"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more individual Technology Class which each represent a technology class in which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Applicable Technology Classes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#audience -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#audience">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Audience"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to the targeted audiences or groups for this view.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Audience</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#author -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#author">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Organization"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Party"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies references to one or more individuals that authored the submitted, contributed, or modified the Weakness, View, Category, or CompoundElement.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#authorOrganization -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#authorOrganization">
        <rdfs:range rdf:resource="http://www.oasis-open.org/ciq_xpil#Organization"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more references tothe organization which submitted, contributed, or modified the Weakness, View, Category, or CompoundElement</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author Organization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#backgroundDetails -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#backgroundDetails">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#BackgroundDetail"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more Background Details, each of which holds information regarding the entry or any technologies that are related to it, where the background information is not related to the nature of the entry itself. It should be filled out where appropriate.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Background Details</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#blackBoxDefinitons -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#blackBoxDefinitons">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#BlackBoxDefinition"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  one or more Black Box Definitions, each of which describes the weakness from an external perspective, 	meaning that the view includes no knowledge of how the software is processing data other than what can be inferred from observing the software&apos;s behavior.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black Box Definitions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#block -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#block">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Block"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a block of text</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Block</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#categories -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#categories">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Category"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  zero or more Category 	elements. Each Category element represents what used to be referred to in CWE as a &quot;Grouping&quot; entry. That is, a category is now a collection of 	weaknesses based on a common attribute, such as CWE-310 Cryptographic Issues or CWE-355 User Interface Security Issues.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Categories</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#chains -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#chains">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#CompoundElement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifes a list of the individual Chains this relationship pertains to.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Chains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#commonConsequences -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#commonConsequences">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Consequence"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more  the common consequences associated with this weakness.  This should be included and completed as much as possible for all weaknesses.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Consequences</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#compoundElements -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#compoundElements">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#CompoundElement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  zero or more Compound_Element elements. 
Each Compound_Element represents a meaningful 	aggregation of several weaknesses, as in a chain like CWE-690: CWE-252 Unchecked Return Value can result in CWE-476 NULL Pointer Dereference or as in a composite like CWE-352 Cross-Site Request Forgery.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CompoundElements</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#consequenceNote -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#consequenceNote">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines additional commentary about this consequence.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequence Note</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#consequenceTechnicalImpact -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#consequenceTechnicalImpact">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#TechnicalImpact"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the technical impacts that can arise if an attacker attempts to exploit this weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Technical Impact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#contentHistory -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#contentHistory">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#HistoryRecord"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more Content History elements used to keep track of the author of the weakness entry and anyone who has made modifications to the content.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Content History</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#demoExampleRefs -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#demoExampleRefs">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Reference"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more References, each of which provide further reading and insight into this demonstrative example. This should be filled out when appropriate.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">References</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#demonstrativeExamples -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#demonstrativeExamples">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#DemonstrativeExample"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Demonstrative Examples, each of which should contain an example illustrating the problem being described in this weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Demonstrative Examples</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#detectionMethods -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#detectionMethods">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#DetectionMethod"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a one or more 	Detection Method which identify methods that may be employed to detect this weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detection Methods</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#enablingExploitationFactors -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#enablingExploitationFactors">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ExploitationFactor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Enabling Factor for Exploitation, each of which points out conditions or factors that could increase the likelihood of exploit for this weakness. This should be filled out for most weakness bases.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Enableing Factors for Exploitation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#extendedDescription -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#extendedDescription">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#StructuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a place for details important to the description of this entry  to be included that are not necessary to convery the fundamental concept behind the entry.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Extended Description</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#maintenanceNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#maintenanceNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more maintenance notes associated with the View.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Maintenance Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#methodEffectivenessNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#methodEffectivenessNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a description of the strengths and shortcomings of this detection method. This should be filled out for some weakness classes and bases.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Method Effectiveness Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#mitigationEffectivenessNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#mitigationEffectivenessNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the strengths and shortcomings of this mitigation on a specific weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mitigation Effectiveness Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#modesOfIntroduction -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#modesOfIntroduction">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#IntroductionMode"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the typical scenarios under which this weakness can be introduced into the software. It should be filled out as needed.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modes of Introduction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#nonPrimaryViews -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#nonPrimaryViews">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#View"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the list of non-ChildOf Views to which this relationship pertains.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship Views</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#objective -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#objective">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ViewObjective"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a description the perspective from which this View is constructed.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Objective</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#observedExamples -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#observedExamples">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ObservedExample"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more ObservedExample elements, each of which is a reference to a specific observed instance of this weakness in the real world; typically this will be a CVE reference.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observed Examples</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#otherNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#otherNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or ore other notes assoicated with the View.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Other Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#potentialMitigations -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#potentialMitigations">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Mitigation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the potential mitigations associated with this weakness. It contains one or more mitigation subelements which each represent individual mitigations for this weakness. This should be included and completed to the extent possible for all weakness bases and variants.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Potential Mitigations</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#primaryViews -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#primaryViews">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#View"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the list of Views to which this relationship pertains, where the relationship represents the primary ChildOf relationship for this weakness entry.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Primary Parent/Child Views</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceAuthor -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#referenceAuthor">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Organization"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Party"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to one or more individuals or organizations that authored the material</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#referencePublisher -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#referencePublisher">
        <rdfs:range rdf:resource="http://www.oasis-open.org/ciq_xpil#Organization"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the publisher of the reference material, if one exists</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Publisher</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#references -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#references">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Reference"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more sources from which more information and deeper insight can be obtained, such as a 	research paper or an excerpt from a publication</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">References</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#relatedAttackPatterns -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#relatedAttackPatterns">
        <rdfs:range rdf:resource="http://capec.mitre.org/capec#AttackPattern"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more references to CAPEC entries which will identify related attack patterns to this weakness. This should be filled out to the extent possible for most weaknesses.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Related Attack Patterns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#relationshipNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#relationshipNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more relationship notes associated with the View</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#relationships -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#relationships">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Category"/>
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#CompoundElement"/>
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#View"/>
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Weakness"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more associations between this structure, whether it is a Weakness, Category, or Compound_Element and another structure.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationships</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#researchGaps -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#researchGaps">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#ResearchGap"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more gaps in the research associated with this Weakness, Category, View, or Compound Element</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Research Gaps</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#subMitigations -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#subMitigations">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Mitigation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more sub-mitigations</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Submitigations</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#taxonomyMappings -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#taxonomyMappings">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#TaxonomyMapping"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Taxonomy Mappings which provide a mapping from this entry to an entry in a different taxonomy.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Taxonomy Mappings</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#terminologyNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#terminologyNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies zero or more notes that contain a discussion of terminology issues related to this weakness.   It is different from Alternate Terms, which is focused on discussion of specific terms that are commonly used. It should be filled out in any entry for which there is no established terminology, or if there are multiple uses of the same key term.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Terminology Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#theoreticalNotes -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#theoreticalNotes">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Note"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies one or more Theoretical Notes that each describe the weakness using vulnerability theory concepts. It should be filled out as needed, especially in cases where the application of 	vulnerability theory is not necessarily obvious for the weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Theoretical Notes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#views -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#views">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#View"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  zero or more View elements.  Each View element represents a perspective with which one might look at the weaknesses in CWE. CWE-699 Development View and CWE-1000 Research View are two examples of Views.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Views</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#vulnerabilityReference -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#vulnerabilityReference">
        <rdfs:range rdf:resource="http://nvd.nist.gov/cve#Vulnerability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a vulnerability entity, typically as a Common Vulnerabilty Enumeration (CVE) entry</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vulnerability Reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#weaknessOrdinality -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#weaknessOrdinality">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#WeaknessOrdinality"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies when this entry is primary - where the weakness exists independent of other weaknesses, or when this entry might be resultant - where the weakness is typically related to the presence of some other weaknesses. This should be filled out for all Weakness Base and 	Variant entries.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Weakness Ordinality</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#weaknesses -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#weaknesses">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#Weakness"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines zero or more Weakness entities that represents an actual weakness entry in CWE, such as CWE-311 Failure to Encrypt Sensitive Data or CWE-326 Weak Encryption.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Weakness</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cwe.mitre.org/cwe#whiteBoxDefinitions -->


    <owl:ObjectProperty rdf:about="http://cwe.mitre.org/cwe#whiteBoxDefinitions">
        <rdfs:range rdf:resource="http://cwe.mitre.org/cwe#WhiteBoxDefinition"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more White Box Definitions, each of which describes the weakness from a white box perspective, meaning that the view includes the knowledge of control flow, data flow, and all other inner workings of the software in which the weakness exists.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">White Box Definitions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://cwe.mitre.org/cwe#abstraction -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#abstraction">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the abstraction level for this weakness.  The abstractions levels for Compound Elements and Weaknesses are the same. For example, if the Compound Element is a chain, and all elements of the chain are Class level, then the Compound Element Abstraction attribute is Class. 

Acceptable values are &quot;Class&quot;, which is the most abstract type of Weakness such as CWE-362 Race Conditions, &quot;Base&quot; which is a more specific type of weakness that is still mostly independent of a specific resource or technology such as CWE-567 Unsynchronized Access to 	Shared Data, and &quot;Variant&quot; which is a weakness specific to a particular resource, technology or context.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Abstraction</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#affectedResources -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#affectedResources">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more system resources affected by this 	entry.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Affected Resources</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#authorName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#authorName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the names of one or more individuals that authored the submitted, contributed, or modified the Weakness, View, Category, or CompoundElement.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#authorOrganizationName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#authorOrganizationName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the name of the organization which submitted, contributed, or modified the Weakness, View, Category, or CompoundElement</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author Organization Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#blockNature -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#blockNature">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the nature of the content contained within the Block.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Block Nature</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#casualNature -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#casualNature">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the nature of the underlying cause of the weakness. Is it an implicit underlying weakness or is it an issue of behavior on the part of the software developer? Appropriate values are 	either Implicit, occurring regardless of developer behavior, or Explicit, an explicit weakness resulting from behavior of the developer.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Casual Nature</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#categoryName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#categoryName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a descriptive name used to give the reader an idea of what the commonality is amongst the children of this category.  All words in the name should be capitalized except for articles and prepositions unless they begin or end the name.  Subsequent words in a hyphenated chain are also not capitalized.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Category Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#causalNature -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#causalNature">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the nature of the underlying cause of the weakness. Is it an implicit underlying weakness or is it an issue of behavior on the part of the software developer? Appropriate values are either Implicit, occurring regardless of developer behavior, or Explicit, an explicit weakness resulting from behavior of the developer.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nature of Cause</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#code -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#code">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a line of code.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#codeExampleLanguage -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#codeExampleLanguage">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the programming language being used in the following block of Code</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Code Language</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#comment -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#comment">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a comment in code</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comment</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#compoundElementName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#compoundElementName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a descriptive name used to give the reader an idea of the meaning behind the compound weakness structure. 

All words in the name should be capitalized except for articles and prepositions unless they begin or end the name. Subsequent words in a hyphenated chain are also not capitalized.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Element Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#compoundElementStructure -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#compoundElementStructure">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the structural nature of this compound element - that is, composed of other weaknesses concurrently, as in a composite, or consecutively, as in a chain.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compound Element Structure</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#consequenceId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#consequenceId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the value for the related Common Consequence entry identifier. However, Common Consequences across CWE with the same ID should only vary in small details.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequence Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#consequenceScope -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#consequenceScope">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines an individual consequence that may be associated with this weakness. Possible effects are detailed in 	Consequence Effect.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Consequence Scope</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#contributionMode -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#contributionMode">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines whether the contribution was part of feedback given to the CWE team or actual content that was donated.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Contribution Mode</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#demoExampleId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#demoExampleId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  the identifier for the related Demonstrative Example. Only one Demonstrative Example ID can exist for each Demonstrative Example (ex: DX-1). However, Demonstrative Examples across CWE with the same ID should only vary in small details.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Demonstration Example Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#descriptionSummary -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#descriptionSummary">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a minimalistic description which provides the information necessary to understand the primary focus of this entry.  The description text should be short and should limit itself to describing the key points that define this entry.  Further explanation can be included in the extended description element.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Description Summary</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#detectionMethodId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#detectionMethodId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines an identifier for the related Detection Method. Only one Detection_Method_ID element can exist for each Detection Method element (ex: DM-1). However, Detection Methods across CWE with the same ID should only vary in small details.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#dictionaryLink -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#dictionaryLink">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies an external link to the weakness contained at the Common Weakness Enumeration (CWE) dictionary.  The link should be in the following form:  http://cwe.mitre.org/data/definitions/{weakness-id}.html, where {weakness-id) should be the integer Identifier of the specific weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dictionary Link</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#entryComment -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#entryComment">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines any comments regarding the content of this weakness entry, such as assumptions made, reasons for omitting 	elements, contact information, pending questions, etc.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comments</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#entryDate -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#entryDate">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the date on which the content was authored, contributed, or modified in YYYY-MM-DD format.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Entry Date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#entryType -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#entryType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the type of the Content History.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Entry Type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#environmentName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#environmentName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the name of the Environment we are identifying.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Environment Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#exampleBody -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#exampleBody">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a description which should hold  the code and some explanatory information for the reader.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Example Body</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#externalLink -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#externalLink">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a valid URL where more information regarding this example can be obtained.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Link</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#form -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#form">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the form of the weakness.

A Compound Element represents a meaningful aggregation of several weaknesses, as in a chain like CWE-690: CWE-252 Unchecked Return Value can result in CWE-476 NULL Pointer Dereference or as in a composite like CWE-352 Cross-Site Request Forgery.  

A Category represents a collection of weaknesses sharing a common attribute, such as CWE-310 Cryptographic Issues or CWE-355 User Interface Security Issues. The shared attribute may any number of things including, but not limited to, environment (J2EE, .NET), functional area (authentication, cryptography) and the relevant resource (credentials management, certificate issues).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Form</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#formattedIdentifier -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#formattedIdentifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the Common Weakness Enumeration (CWE) identifier associated with the weakness.  The weakness identifier should follow the CWE identifier pattern of &quot;CWE-nnnn&quot; where &apos;nnnnn&apos; specifies an numerically incrementing number.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Weakness Enumeration (CWE) identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#functionalAreas -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#functionalAreas">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more descriptions of the functional area of the software in which the 	weakness is most likely to occur. For example,  CWE-178 Failure to Resolve Case Sensitivity is likely to occur in functional areas of software related to file processing and credentials.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Functional Areas</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#hardwareArchitectureClassDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#hardwareArchitectureClassDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines architectures in which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Architecture Class Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#hardwareArchitectureName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#hardwareArchitectureName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  the name of the hardware architectures on which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#id -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#id">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the Common Weakness Enumeration (CWE) Identifier for this entry.   It will be static for the lifetime of the entry. In the event that this entry becomes deprecated, the ID will not be reused and a pointer will be left in this entry to the replacement.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Common Weakness Enumeration (CWE) Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#impactDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#impactDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a description of the technical impact</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Impact Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#impactType -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#impactType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the type of impact</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Impact Type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#introText -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#introText">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the context and setting surrounding the example to add clarity for the reader.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Intro text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#langClass -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#langClass">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a description of a type of language in which this entry is likely to exist. The type may be specified by a common property, such as Languages with Automated Memory Management or Languages which support Format Strings, or it may describe a more general class of language such as All Interpreted Languages or Pseudo Code.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Language Class</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#langName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#langName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  the name of the  programming language in which this entry may occur.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Language Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#likelihood -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#likelihood">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a rough estimate at the likelihood of exploitation of an exposed weakness. Many factors can impact this value which is why it should only be regarded as an approximation.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Likelihood@en-US}</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#localReferenceId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#localReferenceId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies an identifier for the related Local	Reference. Only one Local Reference ID element can exist for each Reference element (ex: R.78.1). Text citing this reference should use the format [R.78.1] where &quot;78&quot; is the CWE identifier and &quot;1&quot; is the ordinal value of the reference (e.g., 1 being the first, 2 being the second).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Local Reference Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mappedNodeId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mappedNodeId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the ID of the entry to which this weakness is being mapped in taxonomy Taxonomy Name.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mapped Node Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mappedNodeName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mappedNodeName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines athe name of the entry to which this weakness is being mapped in taxonomy Taxonomy_Name.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mapped Node Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mappingFit -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mappingFit">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines how close to the original taxonomy this node was mapped.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mapping Fit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#methodDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#methodDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines some context of how this Detection Method can be applied to a specific weakness. This should be filled out for some 	weakness classes and bases.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Method Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#methodEffectiveness -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#methodEffectiveness">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines how effective the detection method may be in detecting the associated weakness. This assumes the use of best-of-breed tools, analysts, and methods. There is limited consideration for financial costs, labor, or 	time.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Method Effectiveness</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#methodName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#methodName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a name for the particular weakness detection method. This should be filled out for some weakness classes and bases.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Method Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mitigationDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mitigationDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the description of this individual mitigation.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mitigation Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mitigationEffectiveness -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mitigationEffectiveness">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines how effective the mitigation may be in preventing the weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mitigation Effectiveness</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mitigationId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mitigationId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the value for the related Mitigation entry identifier as a string. Only one Mitigation_ID element can exist for each Mitigation element (ex: MIT-1). However, Mitigations across CWE with the same ID should only vary in small details.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mitigation Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mitigationPhase -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mitigationPhase">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  the development life cycle phase during which this particular mitigation may be applied.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mitigation Phase</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#mitigationStrategy -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#mitigationStrategy">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a general strategy for protecting a system to which this mitigation contributes</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mitigation Strategy</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#modificationImportance -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#modificationImportance">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines how significant the modification is to the weakness with regard to the meaning and interpretation of the weakness.  If a modification has a value of Critical, then the meaning of the entry or how it might be interpreted has changed and requires attention from anyone previously dependent on the weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modification Importance</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#modificationSource -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#modificationSource">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines whether this modification was created by a CWE team member or provided by an external party.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modification Source</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#noteType -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#noteType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the type of Note.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Note Type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#observationDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#observationDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a product independent description of the example being cited. The description should present an unambiguous correlation between the example being described and the weakness which it is meant to exemplify. It should also be short and easy to understand.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#ordinality -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#ordinality">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies whether this weakness exists independent of other weaknesses, 	Primary, or whether it is the result of the presence of some other weaknesses, Resultant.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ordinality</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#ordinalityDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#ordinalityDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies context in which this entry is either Primary or Resultant, depending on the corresponding value for Ordinality.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#osClass -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#osClass">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines families or groups of operating systems in which this weakness is likely to exist. Do not fill this in if a specific Operating System was specified.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operating System Class Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#osName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#osName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the name of the operating systems in which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#paradigmName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#paradigmName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the name of the architecture  paradigm.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paradigm Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#prevalence -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#prevalence">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the prevalence with which this weakness may occur within code.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Prevalence</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#previousEntryName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#previousEntryName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the name by which this entry was previously known.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Previous Names</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceAuthorName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceAuthorName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the names of one or more an individuals that authored the material being referenced.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Author Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceDate -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceDate">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the date when the reference was 	included in the entry. This provides the reader with a time line for when the material in the reference, usually the link, was valid. The date should be of the format YYYY-MM-DD.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceEdition -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceEdition">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the edition of the material being 	referenced in the event that multiple editions of the material exist. This will usually only be useful for book references.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Edition</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the identifier for the related Reference  entry identifier as a string. Only one Reference_ID element can exist for each Reference element (ex: REF-1). However, References across CWE with the same ID should only vary in small details. Text citing this reference should use the local reference ID, as this ID is only for reference library related consistency checking and maintenance.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceIdentifier -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceIdentifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies  the identifier for the example being cited. For example, if a CVE is being cited it should be of the standard CVE identifier format, such as CVE-2005-1951 or CVE-1999-0046.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceLink -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceLink">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the URL for the material being 	referenced, if one exists. This should always be used for web references, and may optionally be used for book and other publication references.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reference Link</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referencePubDate -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referencePubDate">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines  the date when the reference was published YYYY.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Published Date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referencePublication -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referencePublication">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the publication source of the reference material, if one exists.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Publication</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referencePublisherName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referencePublisherName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the publisher of the reference material, if one exists.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Publisher</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceSection -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceSection">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a means of identifying the exact location of the material inside of the publication source, such as the relevant pages of a research paper, the appropriate chapters from a book, etc. This is useful for both book references and internet references.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#referenceTitle -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#referenceTitle">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the title of the material being referenced. It is not required if the material does not have a title.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#relationshipNature -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#relationshipNature">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the nature of the relationship between this element and the target element, such as ChildOf, HasMember or Requires to name a few.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship Nature</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#relativeId -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#relativeId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies an identifier for the related entity.  Only one relative ID element can exist for each entity (ex: R.78.1). Text citing this reference should use the format [R.78.1] where &quot;78&quot; is the entity identifier and &quot;1&quot; is the ordinal value of the reference (e.g., 1 being the first, 2 being the second)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relative ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#relevantProperties -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#relevantProperties">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Relevant Property.  Each Relevant Property element identifies a property that is required by the code or a resource in order to function as specified.  Correctly labeling all of the relevant properties can help to figure out 	what the root cause of a vulnerability might be.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relevant Properties</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#stakeholder -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#stakeholder">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies what types of members of the CWE community might be interested in this view.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stakeholder</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#stakeholderDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#stakeholderDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a description of what properties of this View this particular Stakeholder might find useful.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#status -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#status">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the status level for this weakness.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Status</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#structureType -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#structureType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies how this view is being constructed. 

Valid values are: Implicit Slice = a slice based on a filter criteria; Explicit Slice = a slice based on arbitrary membership, as defined by specific relationships between entries; Graph = a bounded graphical slice based on ChildOf relationships</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Structure Type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#submissionSource -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#submissionSource">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines how the initial information for this entry was obtained.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Submission Source</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#taxonomyName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#taxonomyName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the taxonomy to which this weakness has a similar or equivalent entry.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Taxonomy Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#techName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#techName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a technology class in which this weakness is likely to exist.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Technology Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#termDescription -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#termDescription">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a short description providing context to the term.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Term Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#termName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#termName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the actual term. Each term should follow the same 	conventions as the entry Name attribute.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Term</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#text -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#text">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a paragraph of text.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#textTitle -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#textTitle">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a bold-faced title for a subsequent block of text.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Text Title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#timeOfIntroduction -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#timeOfIntroduction">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies the points of time in the software life cycle at which the weakness may be introduced. If there are multiple points of time at which the weakness may be introduced, then separate Introduction elements should be included for each.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Time of Introduction</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#viewFilter -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#viewFilter">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines an XSL query for identifying 	which elements are members of an implicit slice. This should only be present for implicit slices.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">View Filter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#viewName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#viewName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a descriptive name used to give the reader an idea of what perspective this view represents. 

All words in the name should be capitalized except for articles and prepositions unless they begin or end the name. Subsequent words in a hyphenated chain are also not capitalized.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cwe.mitre.org/cwe#weaknessName -->


    <owl:DatatypeProperty rdf:about="http://cwe.mitre.org/cwe#weaknessName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a name which identifies the entry. 

The name should focus on the weakness being described in the entry and should 	avoid focusing on the attack which exploits the weakness or the consequences of exploiting the weakness. All words in the entry name should be capitalized except for articles and prepositions unless they begin or end the name. Subsequent words in a hyphenated chain are also not capitalized.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://cwe.mitre.org/cwe#ArchitectureParadigm -->


    <owl:Class rdf:about="http://cwe.mitre.org/cwe#ArchitectureParadigm">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#paradigmName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#prevalence"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Characterizes a single Architectural Paradigm in which this entry may exist and with what frequency in the specified paradigm.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Architecture Paradigm</rdfs:label>
    </owl:Class>
    


    <!-- http://cwe.mitre.org/cwe#Audience -->


    <owl:Class rdf:about="http://cwe.mitre.org/cwe#Audience">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#stakeholder"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#stakeholderDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Characterizes a target audience or group for this view.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Audience</rdfs:label>
    </owl:Class>
    


    <!-- http://cwe.mitre.org/cwe#BackgroundDetail -->


    <owl:Class rdf:about="http://cwe.mitre.org/cwe#BackgroundDetail">
        <rdfs:subClassOf rdf:resource="http://cwe.mitre.org/cwe#StructuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines background information regarding the entry or any technologies that are related to it, where the background information is not related to the nature of the weakness itself. It should be filled out where appropriate.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Background Detail</rdfs:label>
    </owl:Class>
    


    <!-- http://cwe.mitre.org/cwe#BlackBoxDefinition -->


    <owl:Class rdf:about="http://cwe.mitre.org/cwe#BlackBoxDefinition">
        <rdfs:subClassOf rdf:resource="http://cwe.mitre.org/cwe#StructuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines one or more Black Box Definitions, each of which describes the weakness from an external perspective, 	meaning that the view includes no knowledge of how the software is processing data other than what can be inferred from observing the software&apos;s behavior.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black Box Definition</rdfs:label>
    </owl:Class>
    


    <!-- http://cwe.mitre.org/cwe#Block -->


    <owl:Class rdf:about="http://cwe.mitre.org/cwe#Block">
        <rdfs:subClassOf rdf:resource="http://cwe.mitre.org/cwe#StructuredText"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines a  Block of structured text</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Block</rdfs:label>
    </owl:Class>
    


    <!-- http://cwe.mitre.org/cwe#Category -->


    <owl:Class rdf:about="http://cwe.mitre.org/cwe#Category">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#extendedDescription"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#categoryName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#descriptionSummary"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#formattedIdentifier"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#id"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#likelihood"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#status"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#timeOfIntroduction"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cwe.mitre.org/cwe#weaknessName"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Characterizes a category entity that represents a collection of weaknesses based on a common attribute, such as CWE-310 Cryptographic Issues or CWE-355 User Interface Security Issues.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cwe.mitre.org/cwe"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Weakness Category</rdfs:label>

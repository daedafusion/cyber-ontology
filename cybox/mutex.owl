  <owl:ObjectProperty rdf:about="#handle">
    <rdfs:comment >Specifies the open Windows handle to the mutex.</rdfs:comment>
    <rdfs:domain rdf:resource="#WindowsMutex"/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/WindowsHandle#WindowsHandle"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Handle</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#mutexName">
    <rdfs:comment >Specifies the name for a named mutex object.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Mutex"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Mutex Name</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#named">
    <rdfs:comment >Specifies whether the Mutex is named.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#Mutex"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Named</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#securityAttributes">
    <rdfs:comment >Specifies the Windows security attributes for the mutex.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>

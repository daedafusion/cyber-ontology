<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:netPacketObj="http://cybox.mitre.org/objects/NetworkPacket#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://cybox.mitre.org/objects/NetworkPacket">
  <owl:Ontology rdf:about="http://cybox.mitre.org/objects/NetworkPacket">
    <owl:imports rdf:resource="http://cybox.mitre.org/objects/Address"/>
    <owl:imports rdf:resource="http://cybox.mitre.org/cybox_core"/>
    
    <rdfs:label xml:lang="en-US">Cyber Observable eXpression (CybOX) Network Packet Ontology</rdfs:label>
    <rdfs:comment xml:lang="en-US">This ontology provides a vocabulary and representation of a Network Packet for the  Cyber Observable eXpression (CybOX).  &#xD;
&#xD;
CybOX  provides a common mechansim (structure and content) to convey the full range of potential cyber observable information and strives to be fully expressive, flexible, extensible, automatable, and as human-readable as possible.&#xD;
&#xD;
The ontology is based on the CybOX Network Packet object schema maintained by The MITRE Corporation copyright 2012-2014.  The version number of the ontology is kept in sync with the schema in order to more easily relate between the ontology and the corresponding schema version.&#xD;
&#xD;
Copyright (c) 2014, DaedaFusion, LLC.  All rights reserved.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2.1.0-000</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="NetworkPacket">
    <rdfs:comment xml:lang="en-US">Characterizes a Network Packet</rdfs:comment>
    <rdfs:label xml:lang="en-US">Network packet</rdfs:label>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cyboxCommon#id"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#hasChanged"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#description"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://cybox.mitre.org/cybox_core#Object"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#discoveryMethod"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#definedEffect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#physicalLocation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#state"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
</rdf:RDF>


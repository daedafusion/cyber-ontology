  </owl:Class>
  <owl:Class rdf:ID="NetworkProtocolField">
    <rdfs:comment >Characterizes a specific field contained in the associated network protocol</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="flagDescription"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;http://cybox.mitre.org/objects/NetworkProtocol&gt;</rdfs:isDefinedBy>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="operationCode"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="fieldName"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="fieldDescription"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    
    
    <rdfs:label >Network Protocol Field</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="protocolStructure">
    <rdfs:range rdf:resource="#NetworkProtocolField"/>
    
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    <rdfs:comment >Specifies the structure of the network protocol</rdfs:comment>
    <rdfs:label >Protocol Structure</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="commandStructure">
    <rdfs:label >Command Structure</rdfs:label>
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:comment ></rdfs:comment>
    <rdfs:range rdf:resource="#ProtocolCommandStructure"/>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="relatedProtocols">
    <rdfs:label >Related Protocols</rdfs:label>
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:range rdf:resource="#NetworkProtocol"/>
    <rdfs:comment >Specifies related network protocols, typically other network protocols that underly the network protocol.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#commandStructureId">
    <rdfs:label >Id</rdfs:label>
    <rdfs:domain rdf:resource="#ProtocolCommandStructure"/>
    <rdfs:comment >Specifies a unique identifier for the command structure</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#fieldName">
    <rdfs:domain rdf:resource="#NetworkProtocolField"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
    <rdfs:comment >Specifies the name of the field in the associated network protocol</rdfs:comment>
    <rdfs:label >Name</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="rfc">
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies one or more Request for Comment (RFC) identifiers associated with the network protocol</rdfs:comment>
    <rdfs:label >RFC</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#encryption">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:comment >Specifies whether the network protocol is encrypted.</rdfs:comment>
    <rdfs:label >Encryption Used</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#moniker">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:comment >Specifies a moniker or informal name/alias by which the protocol is known</rdfs:comment>
    <rdfs:label >Moniker</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#protocolName">
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the formal name of the network protocol</rdfs:comment>
    <rdfs:label >Name</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="protocolData">
    <rdfs:domain rdf:resource="#NetworkProtocolField"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
    <rdfs:comment >Specifies additional data about the field in the assoicated network protocol</rdfs:comment>
    <rdfs:label >Protocol Data</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#flagDescription">
    <rdfs:domain rdf:resource="#NetworkProtocolField"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    
    <rdfs:comment >Specifies a description of the flag field</rdfs:comment>
    <rdfs:label >Flag Description</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#operationCode">
    <rdfs:domain rdf:resource="#NetworkProtocolField"/>
    
    <rdfs:comment ></rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Operation Code</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandType">
    <rdfs:label >Type</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the type of the command structure for the associated network protocol</rdfs:comment>
    <rdfs:domain rdf:resource="#ProtocolCommandStructure"/>
    
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandGroupLabel">
    <rdfs:domain rdf:resource="#ProtocolCommandStructure"/>
    <rdfs:label >Group Label</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies a group label associted with the command structure</rdfs:comment>
    
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="flagValue">
    <rdfs:domain rdf:resource="#NetworkProtocolField"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies one or more values for the flag field</rdfs:comment>
    <rdfs:label >Flag Value</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#fieldDescription">
    <rdfs:domain rdf:resource="#NetworkProtocolField"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies a description of the field in the associated network protocol</rdfs:comment>
    <rdfs:label >Description</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandDescription">
    <rdfs:label >Description</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies a desription of the command structure</rdfs:comment>
    <rdfs:domain rdf:resource="#ProtocolCommandStructure"/>
    
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#osiLayer">
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >OSI Layer</rdfs:label>
    <rdfs:comment >Specifies the layer in the OSI stake that the network protocol operates at.</rdfs:comment>
    
    
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#commandStructureName">
    <rdfs:domain rdf:resource="#ProtocolCommandStructure"/>
    <rdfs:label >Name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the name of the command structure</rdfs:comment>
    
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="encryptionType">
    <rdfs:domain rdf:resource="#NetworkProtocol"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the type of encryption utilized by the network protocol</rdfs:comment>
    <rdfs:label >Encryption Type</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
  </owl:DatatypeProperty>
</rdf:RDF>


            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cybox.mitre.org/objects/NetworkSocket#options.IP_TOS"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>

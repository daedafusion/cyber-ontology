            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://cybox.mitre.org/objects/NetworkSocket#options.SO_DONTLINGER"/>

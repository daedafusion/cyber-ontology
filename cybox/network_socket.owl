    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#localAddress -->

    <owl:ObjectProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#localAddress">
        <rdfs:range rdf:resource="http://cybox.mitre.org/objects/Socket_Address#SocketAddress"/>
        <rdfs:comment>Specifies the IP address and port for the socket on the local machine.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Local Address</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#remoteAddress -->

    <owl:ObjectProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#remoteAddress">
        <rdfs:range rdf:resource="http://cybox.mitre.org/objects/Socket_Address#SocketAddress"/>
        <rdfs:comment>Specifies the IP address and port for the socket on the remote machine.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Remote Address</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#addressFamily -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#addressFamily">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the address family (AF_*) that the socket is configured for.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Address Family</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#domainFamily -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#domainFamily">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the communication domain (PF_*) of the socket.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Domain Family</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#isBlocking -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#isBlocking">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies whether or not the socket is in blocking mode.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Is Blocking</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#isListening -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#isListening">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies whether or not the socket is in listening mode.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Is Listening</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.IP_MULTICAST_IF -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.IP_MULTICAST_IF">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the interface over which outgoing multicast datagrams should be sent (AF_INET / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>IP_MULTICAST_IF Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.IP_MULTICAST_IF2 -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.IP_MULTICAST_IF2">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the interface over which outgoing multicast datagrams should be sent (AF_INET6 / SOCK_DGRAM or SOCK_RAW) .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>IP_MULTICAST_IF2 Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.IP_MULTICAST_LOOP -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.IP_MULTICAST_LOOP">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies that the sending host receives a copy of an outgoing multicast datagram (AF_INET / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>IP_MULTICAST_LOOP Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.IP_TOS -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.IP_TOS">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies Type of Service (TOS) and Precedence in the IP header (AF_INET).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>IP_TOS Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_BROADCAST -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_BROADCAST">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifes to enable the socket for issuing messages to a broadcast address (AF_INET / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO BROADCAST Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_CONDITIONAL_ACCEPT -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_CONDITIONAL_ACCEPT">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies to allow an application to decide whether or not to accept an incoming connection on a listening socket (Windows only).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_CONDITIONAL_ACCEPT Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_DEBUG -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_DEBUG">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies if low-level debugging is active.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_DEBUG Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_DONTLINGER -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_DONTLINGER">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Complement of SO_LINGER.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_DONTLINGER Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_DONTROUTE -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_DONTROUTE">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies to bypass normal routing mechanisms (AF_INET or AF_INET6 ).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_DONTROUTE Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_GROUP_PRORITY -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_GROUP_PRORITY">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies the relative priority for the socket in its group (Windows only).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_GROUP_PRORITY Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_KEEPALIVE -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_KEEPALIVE">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies to keep the connection up by sending periodic transmissions (AF_INET or AF_INET6 / SOCK_STREAM).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_KEEPALIVE Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_LINGER -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_LINGER">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies if the system attempts delivery of or discards any buffered data when a close() is issued.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_LINGER Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_OOBINLINE -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_OOBINLINE">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies whether out-of-band data is received inline with normal data (AF_INET or AF_INET6).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_OOBINLINE Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_RCVBUF -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_RCVBUF">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies the size of the receive buffer.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_RCVBUF Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_RCVTIMEO -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_RCVTIMEO">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies the receive timeout value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_RCVTIMEO Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_REUSEADDR -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_REUSEADDR">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies if the local socket address can be reused (AF_INET or AF_INET6 / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_REUSEADDR Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_SNDBUF -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_SNDBUF">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies size of the send buffer.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_SNDBUF Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_SNDTIMEO -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_SNDTIMEO">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies the send timeout value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_SNDTIMEO Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_TIMEOUT -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_TIMEOUT">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies the socket timeout.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_TIMEOUT Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.SO_UPDATE_ACCEPT_CONTEXT -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.SO_UPDATE_ACCEPT_CONTEXT">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment>Specifies to update the properties of the socket which are inherited from the listening socket (Windows only).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>SO_UPDATE_ACCEPT_CONTEXT Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#options.TCP_NODELAY -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#options.TCP_NODELAY">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Specifies TCP will send data immediately instead of using the Nagle delay algorithm (AF_INET or AF_INET6 / SOCK_STREAM), when set.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>TCP_NODELAY Option</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#protocolType -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#protocolType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the type of IP layer protocol used by the socket.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol"/>
        <rdfs:label>Protocol Type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#socketDescriptor -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#socketDescriptor">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment>Specifies the socket file descriptor value associated with the socket. Negative values are not allowed.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Socket Descriptor</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://cybox.mitre.org/objects/NetworkSocket#socketType -->

    <owl:DatatypeProperty rdf:about="http://cybox.mitre.org/objects/NetworkSocket#socketType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the type of socket being characterized.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://cybox.mitre.org/objects/NetworkSocket"/>
        <rdfs:label>Socket Type</rdfs:label>
    </owl:DatatypeProperty>
    



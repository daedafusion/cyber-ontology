  <owl:ObjectProperty rdf:about="#remoteAddress">
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/Socket_Address#SocketAddress"/>
    <rdfs:label >Remote Address</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    
    <rdfs:comment >Specifies the IP address and port for the socket on the remote machine.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#localAddress">
    <rdfs:comment >Specifies the IP address and port for the socket on the local machine.</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/Socket_Address#SocketAddress"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Local Address</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#options.IP_MULTICAST_LOOP">
    <rdfs:label >IP_MULTICAST_LOOP Option</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment >Specifies that the sending host receives a copy of an outgoing multicast datagram (AF_INET / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_KEEPALIVE">
    <rdfs:comment >Specifies to keep the connection up by sending periodic transmissions (AF_INET or AF_INET6 / SOCK_STREAM).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_KEEPALIVE Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#domainFamily">
    
    <rdfs:comment >Specifies the communication domain (PF_*) of the socket.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Domain Family</rdfs:label>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_DONTLINGER">
    <rdfs:comment >Complement of SO_LINGER.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_DONTLINGER Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_BROADCAST">
    <rdfs:label >SO BROADCAST Option</rdfs:label>
    <rdfs:comment >Specifes to enable the socket for issuing messages to a broadcast address (AF_INET / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_RCVBUF">
    <rdfs:comment >Specifies the size of the receive buffer.</rdfs:comment>
    <rdfs:label >SO_RCVBUF Option</rdfs:label>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_DONTROUTE">
    <rdfs:comment >Specifies to bypass normal routing mechanisms (AF_INET or AF_INET6 ).</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_DONTROUTE Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.IP_MULTICAST_IF">
    <rdfs:label >IP_MULTICAST_IF Option</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the interface over which outgoing multicast datagrams should be sent (AF_INET / SOCK_DGRAM or SOCK_RAW).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:domain rdf:resource="#NetworkSocket"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_SNDBUF">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    <rdfs:comment >Specifies size of the send buffer.</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_SNDBUF Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_GROUP_PRORITY">
    <rdfs:comment >Specifies the relative priority for the socket in its group (Windows only).</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_GROUP_PRORITY Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.TCP_NODELAY">
    <rdfs:comment >Specifies TCP will send data immediately instead of using the Nagle delay algorithm (AF_INET or AF_INET6 / SOCK_STREAM), when set.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >TCP_NODELAY Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_OOBINLINE">
    <rdfs:comment >Specifies whether out-of-band data is received inline with normal data (AF_INET or AF_INET6).</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_OOBINLINE Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#isListening">
    <rdfs:comment >Specifies whether or not the socket is in listening mode.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Is Listening</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_LINGER">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    <rdfs:label >SO_LINGER Option</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    
    <rdfs:comment >Specifies if the system attempts delivery of or discards any buffered data when a close() is issued.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.IP_MULTICAST_IF2">
    <rdfs:label >IP_MULTICAST_IF2 Option</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the interface over which outgoing multicast datagrams should be sent (AF_INET6 / SOCK_DGRAM or SOCK_RAW) .</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#options.SO_RCVTIMEO">
    <rdfs:comment >Specifies the receive timeout value.</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >SO_RCVTIMEO Option</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#socketDescriptor">
    <rdfs:comment >Specifies the socket file descriptor value associated with the socket. Negative values are not allowed.</rdfs:comment>
    <rdfs:domain rdf:resource="#NetworkSocket"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:label >Socket Descriptor</rdfs:label>
  </owl:DatatypeProperty>

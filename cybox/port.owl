      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="isRegistered"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#state"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:isDefinedBy rdf:resource=""/>
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#definedEffect"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#hasChanged"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:subClassOf rdf:resource="http://cybox.mitre.org/cybox_core#Object"/>
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#physicalLocation"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
<!--     <rdfs:subClassOf>
      <owl:Restriction>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxCardinality>
        <owl:onProperty rdf:resource="http://cybox.mitre.org/cybox_core#discoveryMethod"/>
      </owl:Restriction>
    </rdfs:subClassOf> -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="portNumber"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:displayNameTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Port ${portNumber}</edt:displayNameTemplate>
    <rdfs:label xml:lang="en-US">Port</rdfs:label>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >cybox</edt:namespacePrefix>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="relatedProtocol">
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >cybox</edt:namespacePrefix>
    <rdfs:comment xml:lang="en-US">Specifies the Protocol associated with the port.</rdfs:comment>
    <rdfs:domain rdf:resource="#Port"/>
    <rdfs:label xml:lang="en-US">Related Protocol</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/NetworkProtocol#NetworkProtocol"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#isRegistered">
    <rdfs:label xml:lang="en-US">Registered</rdfs:label>
    <rdfs:domain rdf:resource="#Port"/>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >cybox</edt:namespacePrefix>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:comment xml:lang="en-US">Specifies whether the port is registered in the IANA Service Name and Transport Protocol Port Number Registry.&#xD;
&#xD;
Refer to http://www.iana.org/assignments/service-names-port-numbers/service-names-port-numbers.xhtml for more information.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#portNumber">
    <rdfs:comment xml:lang="en-US">Specifies the actual value of the port.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    <rdfs:domain rdf:resource="#Port"/>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >cybox</edt:namespacePrefix>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label xml:lang="en-US">Port Number</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>


  <owl:DatatypeProperty rdf:about="#type">
    <rdfs:label >Type</rdfs:label>
    <rdfs:domain rdf:resource="#URI"/>
    <rdfs:comment >Specifies the type of URI that is being defined.</rdfs:comment>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >Domain Name</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >General URN</rdf:first>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >URL</rdf:first>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#value">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:label >Value</rdfs:label>
    <rdfs:comment >Specifies the value of the URI.</rdfs:comment>
    <rdfs:domain rdf:resource="#URI"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#urlLabel">

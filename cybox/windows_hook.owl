  <owl:ObjectProperty rdf:about="#hookingModule">
    <rdfs:comment >Specifies the module that contains the hooking function used in the Windows hook procedure that is specified in the Hooking Function Name field.</rdfs:comment>
    <rdfs:label >Hooking Module</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsHook"/>
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/Library#Library"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hookedModule">
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:label >Hooked Modle</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:comment >Specifies the module that is hooked.</rdfs:comment>
    <rdfs:range rdf:resource="http://cybox.mitre.org/objects/Library#Library"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#digitalSignatureHooking">
    <rdfs:comment >Specifies the digital signature of the hooking code.</rdfs:comment>
    
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >Digital Signature Hooking</rdfs:label>
    <rdfs:range rdf:resource="http://cybox.mitre.org/cyboxCommon#DigitalSignature"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#digitalSignatureHooked">
    <rdfs:label >Digital Signature Hooked</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://cybox.mitre.org/cyboxCommon#DigitalSignature"/>
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:comment >Specifies the digital signature of the hooked code.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#handle">
    <rdfs:label >Handle</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range>
      <rdf:Description rdf:about="http://cybox.mitre.org/objects/WindowsHandle#WindowsHandle">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty>
              <owl:DatatypeProperty rdf:about="#hookingFunctionName"/>
            </owl:onProperty>
            <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
            >1</owl:maxCardinality>
          </owl:Restriction>
        </rdfs:subClassOf>
      </rdf:Description>
    </rdfs:range>
    <rdfs:domain rdf:resource="#WindowsHook"/>
    <rdfs:comment >Specifies the handle associated with the  Windows hook procedure.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#hookProcedureType">
    <rdfs:label >Hook Procedure Type</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsHook"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment >Specifies the type (i.e. WH_) of the Windows hook procedure, which refers to the type of event that the hook will intercept.</rdfs:comment>
    
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#hookingFunctionName">
    <rdfs:comment >Specifies the name of the hooking function used by the Windows hook procedure.</rdfs:comment>
    
    <rdfs:label >Hooking Function Name</rdfs:label>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#WindowsHook"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#kernelHookType">
    <rdfs:isDefinedBy rdf:resource=""/>
    
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:comment >Specifies the type of kernel hook being characterized.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Kernel Hook Type</rdfs:label>
    
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#hookDescription">
    <rdfs:comment >Specifies a description of the nature of the hook</rdfs:comment>
    
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Hook Description</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#threadID">
    <rdfs:label >Thread ID</rdfs:label>
    
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:domain rdf:resource="#WindowsHook"/>
    <rdfs:comment >Specifies the ID of the thread associated with the Windows procedure, if applicable.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#hookedFunction">
    <rdfs:comment >Specifies the name of the function that is hooked</rdfs:comment>
    
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label >Hooked Function</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#hookingAddress">
    <rdfs:comment >Specifies the address from where the hooking occurs.</rdfs:comment>
    
    <rdfs:domain rdf:resource="#WindowsKernelHook"/>
    <rdfs:isDefinedBy rdf:resource=""/>
    <rdfs:label >Hooking Address</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedLong"/>
  </owl:DatatypeProperty>
</rdf:RDF>


        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <edt:namespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >oval</edt:namespacePrefix>
    <edt:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</edt:abstract>

    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#requestedCOAs -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#requestedCOAs">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#CourseOfActionBase"/>
        <rdfs:comment>Specifies a reference to a requested Course of Action for this Incident as specified by the producer for the consumer of the Incident Report.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>COA Requested</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#responder -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#responder">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#InformationSource"/>
        <rdfs:comment>Specifies a reference to information about the assigned responder for this Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Responder</rdfs:label>
    </owl:ObjectProperty>
    



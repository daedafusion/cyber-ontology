			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&incident;timeSequence.incidentOpenedDateTime"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>

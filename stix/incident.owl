            <rdfs:domain>
              <owl:Class rdf:ID="COAStatement"/>
            </rdfs:domain>
            <rdfs:domain>
              <owl:Class rdf:ID="ExternalIdentifier"/>
            </rdfs:domain>
            <rdfs:domain rdf:resource="#ImpactAssessmentModel"/>
            <rdfs:domain>
              <owl:Class rdf:ID="AffectedProperty"/>

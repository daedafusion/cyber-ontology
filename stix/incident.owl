    <!-- http://stix.mitre.org/Incident#COAStatement -->

    <owl:Class rdf:about="http://stix.mitre.org/Incident#COAStatement">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://stix.mitre.org/Incident#courseOfAction"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>

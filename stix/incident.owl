    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2002/07/owl#maxCardinality -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#maxCardinality"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://stix.mitre.org/Incident#affectedAssets -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#affectedAssets">
        <rdfs:range rdf:resource="http://stix.mitre.org/Incident#AffectedAsset"/>
        <rdfs:comment>Specifies a reference to the particular assets affected during the Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Affected Assets</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#attributedThreatActors -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#attributedThreatActors">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#ThreatActorBase"/>
        <rdfs:comment>Specifies a reference to Threat Actors asserted to be attributed for this Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Attributed Threat Actors</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#contact -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#contact">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#InformationSource"/>
        <rdfs:comment>Specifies a reference to organizations or personnel involved in this Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Contact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#contributors -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#contributors">
        <rdfs:range rdf:resource="http://cybox.mitre.org/cyboxCommon#Contributor"/>
        <rdfs:comment>Specifies a reference to contributing actors for the Course of Action taken.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Contributors</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#coordinator -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#coordinator">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#InformationSource"/>

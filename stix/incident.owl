        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Coordinator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#courseOfAction -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#courseOfAction">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#CourseOfActionBase"/>
        <rdfs:comment>Specifies a reference to the actual Course of Action.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Course of Action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#externalIdReferences -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#externalIdReferences">
        <rdfs:range rdf:resource="http://stix.mitre.org/Incident#ExternalIdentifier"/>
        <rdfs:comment>Specifies zero or more references to an ID of an incident in a remote system</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>External IDs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#externalImpactAssessmentModel -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#externalImpactAssessmentModel">
        <rdfs:range rdf:resource="http://stix.mitre.org/Incident#ImpactAssessmentModel"/>
        <rdfs:comment>Specifies a reference to impact assessment details utilizing impact assessment characterization models defined externally.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>External Impact Assessment Model</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#history -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#history">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://stix.mitre.org/Incident#COAActionEntry"/>
                    <rdf:Description rdf:about="http://stix.mitre.org/stixCommon#JournalEntry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Specifies a reference to a log of events or actions taken during the handling of the Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>History</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#intendedEffect -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#intendedEffect">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#IntendedEffectStatement"/>
        <rdfs:comment>Specifies a reference to the suspected intended effect of this incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Intended Effect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#leveragedTTPs -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#leveragedTTPs">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#TTPBase"/>
        <rdfs:comment>Specifies a reference to TTPs asserted to be related to this cyber threat Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Leveraged TTPs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#natureOfSecurityEffect -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#natureOfSecurityEffect">
        <rdfs:range rdf:resource="http://stix.mitre.org/Incident#AffectedProperty"/>
        <rdfs:comment>Specifies a reference to how the security properties of the Asset were affected.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Nature of Security Effect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#reporter -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#reporter">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#InformationSource"/>
        <rdfs:comment>Specifies a reference to information about the reporting source of this Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>

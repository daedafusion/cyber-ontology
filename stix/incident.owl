    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#takenCOAs">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#CourseOfActionBase"/>
        <rdfs:comment>Specifies a reference to a Course of Action taken for this Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>COAs Taken</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Incident#victim -->

    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Incident#victim">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://stix.mitre.org/stixCommon#CIQIdentity"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Organization"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Party"/>
                    <rdf:Description rdf:about="http://www.oasis-open.org/ciq_xpil#Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Specifies a reference to information about a victim of this Incident.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Victim</rdfs:label>
    </owl:ObjectProperty>
    



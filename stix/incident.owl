			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&incident;timeSequence.firstMaliciousActionDateTime"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&incident;timeSequence.incidentClosedDatetime"/>

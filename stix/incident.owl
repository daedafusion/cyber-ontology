    <owl:DatatypeProperty rdf:about="http://stix.mitre.org/Incident#countAffected">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Specifies the number of assets of this type affected.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Incident"/>
        <rdfs:label>Count Affected</rdfs:label>
    </owl:DatatypeProperty>
    



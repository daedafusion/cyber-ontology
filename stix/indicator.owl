        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OVAL Variables Generator</rdfs:label>
    </owl:ObjectProperty>
    



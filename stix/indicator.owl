        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to the time window for which this indicator is valid.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">valid time position</rdfs:label>
    </owl:ObjectProperty>
    



        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a confidence assertion in the accuracy of this sighting.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sighting Confidence</rdfs:label>
    </owl:ObjectProperty>
    



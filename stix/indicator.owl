        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OVAL Definitions Test</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#ovalDefinitionsVariables -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#ovalDefinitionsVariables">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to one or more OVAL Definition Varaibles</rdfs:comment>

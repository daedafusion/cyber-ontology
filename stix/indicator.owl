        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OVAL Definitions Variables</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#ovalVariables -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#ovalVariables">

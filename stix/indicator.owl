    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://stix.mitre.org/Indicator#efficacy -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#efficacy">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#EfficacyStatement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies an assertion of likely effectiveness of this TestMechanism to detect the targeted cyber Observables. 

The field includes a description of the asserted efficacy of this TestMechanism and a confidence held in the asserted efficacy of this TestMechanism to detect the targeted cyber Observables.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Efficacy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#eventFilter -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#eventFilter">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#EncodedCDATA"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a Snort event filter line in its native format.

The specification should be within a CDATA construct.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Event Filter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#eventSuppression -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#eventSuppression">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#EncodedCDATA"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a Snort event suppression line in its native format. 

The specification should be within a CDATA construct.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Event Suppression</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#indicatedTTP -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#indicatedTTP">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#TTPBase"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to the relevant TTP indicated by this Indicator.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicated TTP</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#indicator -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#indicator">
        <rdfs:range rdf:resource="http://stix.mitre.org/Indicator#Indicator"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to one cyber threat indicator made up of a pattern identifying certain observable conditions as well as contextual information about the patterns meaning, how and when it should be acted on, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Indicator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#iocIndicator -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#iocIndicator">
        <rdfs:range rdf:resource="http://ioc.mandiant.com/IoC#IndicatorOfCompromise"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to the OpenIOC Indicator of Compromise</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">OpenIOC Indicator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#killchainPhases -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#killchainPhases">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#KillChainPhase"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a relevant kill chain phases relevant to the Indicator.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kill Chain Phases</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#likelyImpact -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#likelyImpact">
        <rdfs:range rdf:resource="http://stix.mitre.org/stixCommon#LikelyImpactStatement"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to a characterization of the likely potential impact within the relevant context if this Indicator were to occur.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Likely Impact</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#observation -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#observation">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://cybox.mitre.org/cybox_core#Observable"/>
                    <rdf:Description rdf:about="http://stix.mitre.org/Indicator#CompositeIndicator"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a relevant cyber observable or a multi-part composite Indicator for this Indicator.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#ovalDefinitions -->


    <owl:ObjectProperty rdf:about="http://stix.mitre.org/Indicator#ovalDefinitions">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to one or more OVAL Definitions.</rdfs:comment>

        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a reference to suggested Courses of Action relevant for this cyber threat Indicator.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://stix.mitre.org/Indicator"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Suggested COAs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://stix.mitre.org/Indicator#testMechanisms -->



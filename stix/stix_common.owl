		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCommon;id"/>
				<owl:someValuesFrom rdf:resource="&rdfs;Literal"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&stixCommon;timestamp"/>
